@CHARSET "UTF-8";

a { color: #606060; text-decoration: none; }
a:hover { color: #196dc6; text-decoration: underline; }
:focus { outline-width: 2px !important; outline-style: solid 1px !important; -moz-outline-radius: 4px; }

button { margin: 0; padding: 0; background: none; border: 0; text-decoration: none; overflow: visible; white-space: nowrap; cursor: pointer; _cursor: hand; }

/* 테이블 STYLE */ 
caption  { width: 0; height: 0; font-size: 0; line-height: 0; overflow: hidden; visibility: hidden; } 
table  { width: 100%; border-collapse: collapse; border: none; color: #636363; }
table  th  { white-space: nowrap; text-align: center; letter-spacing: -0.1em; }
table  td  { white-space: normal; }

#admin_header { height:65px; position:relative; z-index:100; background:url(/wcom/img/mng/bg/gnavi_bg.gif) left top repeat-x; }
#admin_header .header_content { position:relative; height:65px; width:980px; margin:0 auto; }
#admin_header .header_content a { color:#fff;}
#adminWrap { min-height:100%; margin:-65px 0 -90px; background:url(/wcom/img/mng/bg/content_bg.gif) left 65px repeat-x;}
* html #adminWrap { height:100%; }
#content { padding:110px 0 110px 0; width:980px; margin:0 auto; }

#admin_footer { height:105px; background:url(/wcom/img/mng/bg/fbg.gif); height:90px; }
#admin_footer .footer_content{ position:relative; width:980px; height:65px; margin:0 auto; padding:25px 5px 0 5px; color:#959595; font-size:0.9em; }


.header_content .logo { position:absolute; z-index:101; width:180px; height:40px; padding-top:15px; }
.header_content .header_menus { position:relative; height:65px; padding-left:180px; z-index:100; text-align:left; }
.header_content .header_menus ul#topmenu { position:relative; float:left; height:65px; background:url(/wcom/img/mng/bg/top_menu_off.gif) right top no-repeat; text-align:left; }
.header_content .header_menus ul#topmenu li { float:left; height:65px; padding:0px 13px; background:url(/wcom/img/mng/bg/top_menu_off.gif) left top no-repeat; text-align:left; }
.header_content .header_menus ul#topmenu li a { float:left; font-weight:bold; line-height:65px; vertical-align:bottom; color:#AFB1BC; text-decoration:none; }
.header_content .header_menus ul#topmenu li a:active,
.header_content .header_menus ul#topmenu li a:focus,
.header_content .header_menus ul#topmenu li a:hover { color:white;}
.header_content .header_menus ul#topmenu li ul.sub_menus { position:absolute; left:0; top:65px; width:100%; height:auto; clear:both; display:none; text-align:left; }
.header_content .header_menus ul#topmenu li ul li { position:relative; float:left; height:auto; border:0px; padding:5px 0px; margin-right:20px; background:none; }
.header_content .header_menus ul#topmenu li ul li a { text-decoration:none; line-height:22px; color:#9FA1AC; white-space:nowrap; }
.header_content .header_menus ul#topmenu li ul li a:active,
.header_content .header_menus ul#topmenu li ul li a:focus,
.header_content .header_menus ul#topmenu li ul li a:hover { color:#7F818C; }
.header_content .header_menus ul#topmenu li.last { }

.header_content .header_menus ul#topmenu li.li1 { }
.header_content .header_menus ul#topmenu li.li2 { }
.header_content .header_menus ul#topmenu li.li3 { }
.header_content .header_menus ul#topmenu li.li4 { }
.header_content .header_menus ul#topmenu li.li5 { }
.header_content .header_menus ul#topmenu li.li6 { }

.header_content .header_menus .login_infor { float:right; padding-top:25px; width:222px; }
.header_content .header_menus .login_infor li.user { float:left; color:#AFB1BC; text-align:left; width:145px; }
.header_content .header_menus .login_infor li.user strong { font-weight:bold; color:#AFB1BC;}
.header_content .header_menus .login_infor li.btn { float:left; width:77px;}


/* 컨텐츠 상단 타이틀 */
#content h1.stitle01_01,
#content h1.stitle01_03 { background:url(/wcom/img/mng/icon/hstyle.gif) left no-repeat; height:auto; color:#2F3743; font-size:16px; padding:0 0 0 20px; margin:0 0 10px 0; border:0px; }

