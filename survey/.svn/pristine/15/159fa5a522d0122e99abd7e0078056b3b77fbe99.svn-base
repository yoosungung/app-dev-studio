@CHARSET "UTF-8";

@font-face {
    font-family:'NanumGothic';
    src: url('/wcom/font/NanumGothic.eot');
    src: local('?'),
         url('/wcom/font/NanumGothic.eot?#iefix') format('embedded-opentype'),
         url('/wcom/font/NanumGothic.woff') format('woff'),
         url('/wcom/font/NanumGothic.ttf') format('truetype');
    font-weight: normal;
    font-style: normal; 
}

* { margin:0; padding:0; }
/* * { vertical-align: middle; }*/
img { border:0px; }
html,
body { height:100%; margin:0; padding:0; color:#707070; font-size:12px; line-height:1.5; font-family: AppleGothic, dotum, Gulim, Tahoma, Verdana , UnDotum, sans-serif; }

ul { list-style:none; }
li { list-style:none; }
 
 table caption{display:none}
 table{table-layout:fixed}
/* IE9 호환성 보기시에 필요한 상태....
button { position:relative; }
button img { position:absolute; left:0px; top:0px; }
*/
button { cursor:pointer; }

a,
a:hover,
a:visited,
a:active,
a:focus,
a:link { font-size:12px; line-height:1.5; text-decoration:none; color:#707070; }

.float_left { float: left; }
.float_right { float: right; }
.display_none { display: none!important; }
.display_block { display: block; }

fieldset { padding:0; margin:0; border:0;}
fieldset legend { position:absolute; left:-1px; top:-1px; width:1px; height:1px; overflow:hidden; line-height:1px; font-size:1px; }
table caption { position:absolute; left:-1px; top:-1px; width:1px; height:1px; overflow:hidden; line-height:1px; font-size:1px; }

h1 { font-size:18px; height:27px; line-height:27px;  }
h2 { font-size:18px; height:27px; line-height:27px;  }
h3 { font-size:14px; height:24px; line-height:24px;  }

.mt30{margin-top:30px;}
.mr3{margin-right:3px;}

.word_reduce { white-space:nowrap; overflow:hidden; text-overflow:ellipsis; }
input[type='checkbox']{vertical-align:middle;}
input[type='radio']{vertical-align:middle;}

.required { font-size:12px; color:#f95e00; margin-left:-7px; }

.txt_center { position:relative; text-align:center; }

.imgr img { opacity:0; }

select.select { vertical-align:bottom; display:inline-block; height:28px; line-height:28px; _line-height:normal; background:#fff; border:1px solid #e0e0e0; color:#666; }


.popup_layer_container .layer_title { display:none; }
.popup_layer_container .map_location_pic { position:relative; width:100%; height:450px; }
.popup_layer_container .map_setup_location_pic { position:relative; width:100%; height:490px; }
.popup_layer_container .map_find_location_pic { position:relative; width:100%; height:480px; }


#map_location_print_area { margin-top:10px; }


.google_marker_label { position:absolute; height:53px; text-align:left; padding-right:7px; cursor:default;}
.google_marker_label .label_text { position:relative; background:url(/wcom/img/content/info_window.png) left top no-repeat transparent; white-space:nowrap; overflow:hidden; text-overflow:ellipsis; }
.google_marker_label .label_text { color:#fff; }
.google_marker_label .label_text a { float:left; height:32px; padding:11px 6px 10px 4px; display:inline; color:#fff; }
.google_marker_label .label_text a.icon { background:url(/wcom/img/content/btn_arrow_blue.png) 4px 14px no-repeat transparent; padding-left:20px; }
.google_marker_label .close_btn { position:absolute; text-align:left; right:0px; padding-top:7px; height:46px; width:7px; background:url(/wcom/img/content/info_window.png) right top no-repeat transparent; }

.popup_layer_container .spCharTypes { position:relative; float:left; display:inline-block; width:100%; margin-top:10px; border:1px solid #cdcdcd; list-style:none; }
.popup_layer_container .spCharTypes li { float:left; display:inline-block; padding:4px 8px; }
.popup_layer_container .spCharTypes li label { cursor:pointer; }
.popup_layer_container .spCharTypes li label.on { font-weight:bold; }


.popup_layer_container .charactor_list { position:relative; float:left; display:inline-block; width:100%; margin:10px 0; border:1px solid #cdcdcd; list-style:none; }
.popup_layer_container .charactor_list li { position:relative; float:left; display:inline-block; padding:4px 8px; }
.popup_layer_container .charactor_list li label { cursor:pointer; }
.popup_layer_container .charactor_list li label.on { font-weight:bold; }


.asc a { color:#1d85c1; padding-right:10px; background:url(/wcom/img/icon/asc.gif) right center no-repeat transparent; }
.desc a { color:#1d85c1; padding-right:10px; background:url(/wcom/img/icon/desc.gif) right center no-repeat transparent; }


/* 에러 페이지 */
.not_found{ background:url(wcom/img/content/notfound.gif); padding:30px; border:1px solid #EBEBEB; }
.not_found .text_center{ text-align:center; border-bottom:1px dashed #DFDFDF; padding-bottom:20px}
.not_found .exclamation_mark{ background:url(/wcom/img/content/exclamationmark.png) no-repeat center center; text-indent:-5000px; height:110px}
.not_found .img_notfound{font-weight:bold; font-size:2.3em; color:#4b4b4b}
.not_found .img_notfound span{color:#ff2626}
.not_found .img_subtext{font-size:1.3em;color:#9a9a9a; font-weight:bold}
.not_found .sub_text{ padding-top:15px}



/* 로그인 */
.log_wrap{display:block; width:618px; height:215px; margin:0 auto 50px;}
.log_wrap .log_box{float:left;width:305px; height:212px; background:#f9fbfb; border:1px solid #e4e7e7; border-top:2px solid #595959; overflow:hidden;}
.log_wrap .login {position:relative; width:240px; margin:30px auto 0; min-height:100%; z-index:2;}
.log_wrap ul.login_items { position:relative; list-style:none; text-align:left; }
.log_wrap ul.login_items li a {}
.log_wrap .login .login_fail_message { color:#ffcaca; }
.log_wrap .login .login_fail_message .login_fail_reason { color:#cee2ff; }
.log_wrap .login .password_change_message { color:#cee2ff; }
.log_wrap .login ul.login_items li.login_id { position:relative; padding-left:15px; padding-top:28px; }
.log_wrap .login ul.login_items li.login_id label { position:absolute; left:-1px; top:-1px; width:1px; height:1px; font-size:1px; overflow:hidden; }
.log_wrap .login ul.login_items li.login_id input { height:22px; line-height:22px;  width:140px; padding:0 5px; border:1px solid #eaeaea; font-size:14px; }
.log_wrap .login ul.login_items li.password { position:relative; padding-left:15px; padding-top:3px; }
.log_wrap .login ul.login_items li.password label { position:absolute; left:-1px; top:-1px; width:1px; height:1px; font-size:1px; overflow:hidden; }
.log_wrap .login ul.login_items li.password input { height:22px; line-height:22px; width:140px; padding:0 5px; border:1px solid #eaeaea; font-size:14px; }
.log_wrap .login ul.login_items li.id_save { position:absolute; left:16px; padding-top:6px; }
.log_wrap .login ul.login_items li.id_save input{vertical-align:middle;}
.log_wrap .login ul.login_items li.auto_login { position:relative; left:110px; padding-top:6px; }
.log_wrap .login ul.login_items li.auto_login input{vertical-align:middle;}
.log_wrap .login ul.login_items li.login_btn { position:absolute; left:169px; top:28px; }
.log_wrap .login ul.login_items li.login_btn button { position:relative; width:57px; height:51px;  margin:0; padding:0; border:0; }
.log_wrap .login ul.login_items li.login_btn button img { float:left;}
.log_wrap .login ul.login_items li.id_find_link { position:absolute; top:0px; right:24px; }
.log_wrap .login ul.login_items li.join_link { position:absolute; top:0px; left:16px; padding-right:14px; background:url(/wcom/img/side/login_link_bar.gif) right center no-repeat; }
.log_wrap .login ul.login_items li.join_link a { font-weight:bold; }
.log_wrap .login ul.login_items li.exp{position:absolute; width:90%; margin:15px 0 0 15px; padding-top:10px; border-top:1px solid #e3e4e4;color:#9a9a9a; font-size:0.9em;}
.log_wrap .ad{ float:right; width:305px; height:215px; overflow:hidden;}
h3.info{width:618px; margin:0 auto;;font-size:14px;}
h4.info{width:618px; margin:0 auto;font-size:12px; font-weight:normal;}
ul.servicelist{overflow:hidden; width:618px; margin:40px auto 0; padding:0; background:url(/wcom/img/bg/bg_log_ul.gif) 45% 0 repeat-y;}
ul.servicelist li{float:left; width:43%; height:21px; margin-bottom:10px; line-height:1.8;padding-left:40px;}
ul.servicelist li.quick{background:url(/wcom/img/icon/icon_quick.gif) 0 0 no-repeat;}
ul.servicelist li.finder{background:url(/wcom/img/icon/icon_finder.gif) 0 0 no-repeat;}
ul.servicelist li.clinic{background:url(/wcom/img/icon/icon_clinic.gif) 0 0 no-repeat; margin:0; margin-bottom:10px; padding:0; padding-left:40px; background-position-x:2px;}
ul.servicelist li.helpme{background:url(/wcom/img/icon/icon_helpme.gif) 0 0 no-repeat;}
ul.servicelist li.partner{background:url(/wcom/img/icon/icon_partner.gif) 0 0 no-repeat; margin:0; margin-bottom:10px; padding:0; padding-left:40px; }
ul.servicelist li.myzeus{background:url(/wcom/img/icon/icon_myzeus.gif) 0 0 no-repeat; background-position-x:1px;}
ul.servicelist li.cafe{background:url(/wcom/img/icon/icon_cafe.gif) 0 0 no-repeat; background-position-x:2px;}


/* 20130325 멘토부분때문에 수정 */
.btn_MinusPlus {height:27px; line-height:27px; vertical-align:bottom;display:inline-block;}
.btn_MinusPlus button{border:0;margin:0;padding:0;background:none}
.btn_MinusPlus button img{vertical-align:bottom}


.fr{float:right}
