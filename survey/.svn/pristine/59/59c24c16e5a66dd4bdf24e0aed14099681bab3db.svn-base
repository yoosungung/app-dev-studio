.tbl_type12 select{padding:5px 0px; line-height:20px; min-width:100px; background:#ededef; border:1px solid #dfdfdf; border-right:1px solid #c9c9c9; border-bottom:1px solid #c9c9c9;}
.tbl_type12 input[type="checkbox"]{margin-right:5px; vertical-align:middle;}
.typeinfo{display:block; padding:5px 15px; font-size:11px; color:#666; background:#fcfcfc; border:1px solid #e1e1e1;}


.crit_item{display:block; position:absolute; top:15px; left:30px; width:6px; height:4px; background:url(../../wcom/img/zeus2014/common/sp_bullet.png) 0px -550px no-repeat;}
.guidebox{padding:0px; margin:0px; height:0px;}
.crit_guide{display:block; position:absolute; top:25px; right:20px; width:62px; height:11px; background:url(../../wcom/img/zeus2014/common/sp_etc.png) -500px -200px no-repeat;}
.layerform .crit_guide{display:inline-block; position:relative; top:0px; left:0px; vertical-align:middle; margin-left:10px;}
.ipstyle{vertical-align:middle; line-height:20px; padding:3px 5px; background:#ededef; border:1px solid #dfdfdf; border-right:1px solid #c9c9c9; border-bottom:1px solid #c9c9c9;}
.btn_input{display:inline-block; vertical-align:middle; margin-left:4px; width:80px; height:27px; line-height:27px; text-align:center; font-size:11px; border:1px solid #e6e6e6; border-bottom:1px solid #c8c8c8;}
.radiobox{display:block;}
.radiobox:after{clear:both; display:block; content:""}
.radiobox li{float:left; width:150px; line-height:26px;}
.radiobox li input[type="radio"]{vertical-align:top; margin-top:5px;}
.radiobox2{display:block;}
.radiobox2:after{clear:both; display:block; content:""}
.radiobox2 li{float:left; line-height:26px;}
.radiobox2 li input{vertical-align:top;}
.radiobox2 li input[type="radio"]{vertical-align:top; margin-top:5px;}
.ipbox{display:block;}
.ipbox:after{clear:both; display:block; content:""}
.ipbox li{float:left; width:30%;}
