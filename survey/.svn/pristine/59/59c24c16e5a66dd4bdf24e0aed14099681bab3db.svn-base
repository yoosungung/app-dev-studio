@charset "utf-8";
/* CSS Document */

.tl{text-align:left !important}
.tc{text-align:center !important}
.tr{text-align:right !important}

.tbl_type01{width:100%; border-top:1px solid #0082c0;}
.tbl_type01.type1{border-top:0;}
.tbl_type01.type2{border-top:1px solid #7f7f7f}
.tbl_type01 th, .tbl_type01 td{border-bottom:1px solid #e2e2e2;}
.tbl_type01 th{position:relative; padding:15px 0px 15px 40px; vertical-align:top; text-align:left; color:#474747; background:#fafafa;}
.tbl_type01 .mustbe{position:absolute; top:21px; left:30px; width:6px; height:4px; text-indent:-9999px; background:url(../../wcom/img/zeus2014/common/mustbe.gif) no-repeat;}
.tbl_type01 td{padding:15px 15px;}
.tbl_type01 td.ptn_guide{padding:0px 0px 0px 40px; border-bottom:1px solid #bbb;}
.tbl_type01 input[type="text"],select{border:1px solid #939393; height:22px;}
.tbl_type01 input[type="radio"]{border:none; vertical-align:middle;}
.tbl_type01 input[type="checkbox"]{border:none; vertical-align:middle;}
.tbl_type01 textarea{border:1px solid #939393;}
.tbl_type01 .input_full{width:100%;}
.tbl_type01 input.mt3{margin-top:3px;}
.tbl_type01 .guide_long{display:inline-block; padding-left:20px; margin-top:5px; font-size:11px; background:url(../../wcom/img/zeus2014/common/sp_bul.png) no-repeat 0 -698px;}
.tbl_type01 .btn_overlap{width:69px; height:22px; margin-left:10px; background:#898989; border:1px solid #5b5b5b; font-size:11px; color:#fff;}
.tbl_type01 .explain{display:block; margin-top:5px; font-size:11px;}
.myzeus .tbl_type01 th{padding-left:20px; width:120px; *width:110px;}
.myzeus .tbl_type01 .mustbe{left:10px;}
.myzeus .tbl_type01 .exp{margin-top:5px;}


.tbl_type02{width:100%; border-top:1px solid #0082c0}
/* .tbl_type02 tbody tr:hover{background:#f7fcff} */
.tbl_type02 th, .tbl_type02 td{height:29px; text-align:center; color:#444;}
.tbl_type02 td.leftcon{text-align:left;}
.tbl_type02 td.leftcon span{display:block; font-size:11px; color:#888f;}
.tbl_type02 th.left_th{background:none; border-bottom:1px solid #ededec; border-right:1px solid #ededec;}
.tbl_type02 th{padding:6px 0;border-bottom:1px solid #bbbdc0; background:#fafafa}
.tbl_type02 td{padding:4px 10px;border-bottom:1px solid #ededec; word-break:break-all;}
.tbl_type02 td a{display:block; max-width:100%;
overflow : hidden;
white-space: nowrap;
text-overflow:ellipsis;
-o-text-overflow:ellipsis;
-ms-text-overflow:ellipsis;
}
.tbl_type02.allview td a{display:block; max-width:100%;
overflow : visible;
white-space:normal;
text-overflow:clip;
-o-text-overflow:clip;
-ms-text-overflow:clip;
}
.tbl_type02 td a.file{text-indent:18px; background:url(../../wcom/img/zeus2014/icon/icon_file.png) no-repeat 0 2px}

.tbl_type02.type1{border-top:0;}
.tbl_type02.type1 th{color:#474747;}

.tbl_type03{width:100%; }
.tbl_type03 tbody tr:hover{background:#f7fcff}
.tbl_type03 th, .tbl_type03 td{border-bottom:1px solid #cfcfcf; text-align:center}
.tbl_type03 th{padding:10px 0;}
.tbl_type03 td{padding:5px 15px; height:21px;}
.tbl_type03 td.sort{padding:5px 0;}
.tbl_type03 td a{float:left; display:block; max-width:94%; line-height:21px;
overflow : hidden;
white-space: nowrap;
text-overflow:ellipsis;
-o-text-overflow:ellipsis;
-ms-text-overflow:ellipsis;
}
.tbl_type03 td .icon_new{margin-left:4px; padding-top:2px;}
.tbl_type03 td.origin{
width:100%;
display:table-cell;
overflow : hidden;
white-space: nowrap;
text-overflow:ellipsis;
-o-text-overflow:ellipsis;
-ms-text-overflow:ellipsis;
}

.tbl_type04{width:100%; }
.tbl_type04 th, .tbl_type04 td{height:21px; padding:20px 15px; border-bottom:1px solid #cfcfcf; text-align:left;}
.tbl_type04 th{ vertical-align:top}
.tbl_type04 td{vertical-align:middle}
.tbl_type04 .exp{margin:10px 0 0;}

.tbl_type05{width:100%; border-top:1px solid #0082c0}
.tbl_type05 tr.notice{background:#f8f8f8;}
.tbl_type05 th{height:37px; text-align:center; border-bottom:1px solid #bbbdc0;}
.tbl_type05 td{padding:8px 20px; line-height:18px; text-align:center; border-bottom:1px solid #ededec;}
/*.tbl_type05 td.date{color:#0082C0;}*/
.tbl_type05 tr.active td{border-bottom:none;}
.tbl_type05 tr.active td a{text-decoration:underline;}
.tbl_type05 td.opentd{padding:0px;}
.tbl_type05 td img{vertical-align:middle; padding:0px 7px 2px 0px;}
.tbl_type05 td a/*, .tbl_type05 td span*/{display:block; max-width:100%; text-align:left;
overflow : hidden;
white-space: nowrap;
text-overflow:ellipsis;
-o-text-overflow:ellipsis;
-ms-text-overflow:ellipsis;
}
.tbl_type05 td a:hover,.tbl_type05 td a:focus,.tbl_type05 td a:active{color:#0082C0; text-decoration:underline;}

.tbl_type05 .openarea{text-align:left; border-top:1px solid #c3c3c3; background:#f2f8ff;}
/*.tbl_type05 .openarea .openarea_tit{padding:12px 20px 12px 45px; color:#0082c0; border-bottom:1px dashed #d6dce2; background:url(../../wcom/img/zeus2014/common/sp_bul.png) no-repeat 22px -928px;}*/
.tbl_type05 .openarea .openarea_tit{padding:12px 20px 12px 20px; color:#0082c0; border-bottom:1px dashed #d6dce2;}
.tbl_type05 .openarea .down_file{display:block; zoom:1;}
