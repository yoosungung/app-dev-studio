.board_type01 .parag01 .rightcon .btn_ex{display:block; width:157px; height:32px; background:url(../../wcom/img/zeus2014/common/sp_btn.png) 0px -350px no-repeat;}
.board_type01 .parag01 .rightcon .btn_reserv1{display:block; width:157px; height:32px; background:url(../../wcom/img/zeus2014/common/sp_btn.png) 0px -800px no-repeat;}
.board_type01 .parag01 .rightcon .btn_reserv2{display:block; width:157px; height:32px; background:url(../../wcom/img/zeus2014/common/sp_btn.png) -200px -800px no-repeat;}
.board_type01 .parag01 .rightcon.type2 li{margin-top:-5px;}
.board_type01 .parag01 p{padding-top:13px; line-height:18px; border-top:1px dashed #ccc;}


