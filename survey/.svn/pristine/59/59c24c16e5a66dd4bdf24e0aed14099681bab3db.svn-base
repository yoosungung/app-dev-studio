.board_type01 .parag01 .rightcon .loca{height:30px; padding-left:24px; background:url(../../wcom/img/zeus2014/common/sp_icon.png) 0px -450px no-repeat;
overflow : hidden;
white-space: nowrap;
text-overflow:ellipsis;
-o-text-overflow:ellipsis;
-ms-text-overflow:ellipsis;}
.board_type01 .parag01 .rightcon .call{height:30px; padding-left:24px; background:url(../../wcom/img/zeus2014/common/sp_icon.png) 0px -500px no-repeat;}
