.tbl_type10 ol:after{clear:both; display:block; content:""}
.tbl_type10 ol li{position:relative; float:left; padding-left:5px;}
.tbl_type10 ol li .rentdate{display:block; position:absolute; bottom:-20px; right:0px; width:55px; text-align:center; font-size:11px; color:#929292;}


.tbl_type12 textarea{position:relative; height:80px;}
.tbl_type12{width:100%; padding-bottom:20px; text-align:left;}
.tbl_type12:after{clear:both; display:block; content:""}
.tbl_type12 .applicant{padding-top:5px; height:37px; line-height:37px; border-bottom:1px solid #e4e4e4;}
.tbl_type12 h3{margin-left:-10px; padding-left:10px; height:48px; line-height:48px; text-align:left; font-size:14px; color:#0cbcc8; background:url(../../wcom/img/zeus2014/common/sp_bullet.png) 0px -482px no-repeat;}
.tbl_type12 th{position:relative; vertical-align:top; padding:10px 20px 5px 40px; *padding:10px 0px 5px 40px; text-align:left;}
