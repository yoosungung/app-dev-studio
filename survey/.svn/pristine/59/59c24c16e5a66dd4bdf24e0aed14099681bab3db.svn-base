.tbl_type05 .openarea .down_file ul{padding:10px 20px 0px 20px;}
.tbl_type05 .openarea .down_file ul li{height:18px; overflow:hidden;}
.tbl_type05 .openarea .down_file ul li a{padding-left:15px; line-height:18px; font-size:11px; background:url(../../wcom/img/zeus2014/icon/icon_file.png) no-repeat 0px 2px;}
.tbl_type05 .openarea .openarea_con{padding:10px 20px; line-height:20px;}
.tbl_type05 .openarea .openarea_writer{padding:10px 20px 20px 20px; text-align:right;}
.tbl_type05 .usefile{padding-left:3px;}


.tbl_type06{width:100%;}
.tbl_type06 th{background:#bcbcbc; color:#fff;}
.tbl_type06 td{border:none;}
.tbl_type06 td.stepname{background:#ddf4ff; text-align:center;}
.tbl_type06 td.stepcon{padding:10px; background:#f7f7f7;}
.tbl_type06 td.stepcon ul li{padding-left:7px; background:url(../../wcom/img/zeus2014/common/sp_bul.png) no-repeat 0px -28px;}
.tbl_type06 td.stepcon span{display:block; padding-left:7px; font-size:11px;}
.tbl_type06 td.arr_next{height:33px; background:url(../../wcom/img/zeus2014/content/partner/arr_next.gif) no-repeat center center;}

.find_id .tbl_type04 th{width:120px; *width:100px; padding-left:50px;}


.tbl_type07{width:100%; border-top:1px solid #808183;}
.tbl_type07 th, .tbl_type07 td{height:21px; padding:10px 15px; border-bottom:1px solid #cfcfcf; text-align:left;}
.tbl_type07 th{color:#0fa5bb; vertical-align:top}
.tbl_type07 td{vertical-align:middle}

.tbl_type08{width:100%;}
.tbl_type08 th, .tbl_type08 td{height:21px; padding:10px 0px; text-align:left; border:none;}
.tbl_type08 th{position:relative; padding-left:10px; color:#000;}
.tbl_type08 td{vertical-align:middle;}
.tbl_type08 input, .tbl_type08 textarea{width:100%; *width:99%; background:#f8f8f8; border:1px solid #b3bbc1;}
.tbl_type08 input, .tbl_type08 select{min-width:150px;}
.tbl_type08 .mustbe{position:absolute; top:21px; left:0px; width:6px; height:4px; text-indent:-9999px; background:url(../../wcom/img/zeus2014/common/mustbe.gif) no-repeat;}

.tbl_type08 .guide_long{display:inline-block; padding-left:20px; margin-top:5px; font-size:11px; background:url(../../wcom/img/zeus2014/common/sp_bul.png) no-repeat 0 -698px;}

.t_type_mento_choose{width:100%;}
.t_type_mento_choose th, .t_type_mento_choose td{padding:5px 0; border:0;text-align:center}
.t_type_mento_choose th{color:#444}
.t_type_mento_choose td{}


.openarea_con table{table-layout:auto; width:100% !important}






/* My제우스 - My 나눔 */
.board_head{display:block; position:relative; width:100%; height:39px; line-height:39px; background:#f9f9f9; border-bottom:1px solid #e0e0e0;}
.board_head:after{clear:both; display:block; content:"";}
.board_head .resultword{float:left; width:450px; padding-left:19px;}
.board_head .resultword span{color:#ff0000;}
.board_head div{display:block; float:right; margin:7px 15px 0px 0px; min-width:60px;}
.board_head div:after{clear:both; display:block; content:"";}

.board_head .minisrch{display:block; position:absolute; top:8px; right:20px; height:24px;}
.board_head .minisrch:after{clear:both; display:block; content:""}
.board_head .minisrch .otherarea{display:block; position:relative; float:left;} 
.board_head .minisrch .inparea{display:block; position:relative; float:left;}
.board_head .minisrch select{float:left; margin-right:5px; width:115px; height:24px; line-height:20px;}
.board_head .minisrch label{display:block; position:absolute; top:6px; left:10px; height:14px; line-height:14px;  font-size:11px; color:#9f9f9f; z-index:50;}
.board_head .minisrch input{float:left; padding-left:10px; width:133px; height:22px; border:1px solid #cdcdcd; border-right:none;}
.board_head .minisrch button{float:left; width:37px; height:24px; background:url(../../wcom/img/zeus2014/common/sp_btn.png) -300px -600px no-repeat;}


/*board_type01*/
