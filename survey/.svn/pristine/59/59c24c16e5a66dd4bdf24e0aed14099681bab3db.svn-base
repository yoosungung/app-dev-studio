.filebox li .existing a:hover, .filebox li .existing a:focus, .filebox li .existing a:active{text-decoration:underline;}
.filebox.type2 li{border-top:1px dashed #eee; padding:8px 0px 5px 35px;}
.filebox.type2 li label{top:10px;}
.filebox.type2 li .crit_item{top:15px;}