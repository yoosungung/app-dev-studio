<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN" "http://tiles.apache.org/dtds/tiles-config_2_1.dtd">
<tiles-definitions>
    
    <!-- 데이터집합 생성 -->
    <definition extends="default" name="settings/datasets/create" template="/WEB-INF/layouts/bootstrap-blank.jspx">
		<put-attribute name="pagemode" value="create" cascade="true"/>
		<put-attribute name="body" value="/WEB-INF/views/settings/datasets/create.jspx"/>
    </definition>
    
    <!-- 데이터집합 목록 -->
    <definition extends="default" name="settings/datasets/list" template="/WEB-INF/layouts/bootstrap.jspx">
        <put-attribute name="activeTab" value="dataset_list" cascade="true"/>
        <put-attribute name="pagemode" value="list" cascade="true"/>
        <put-attribute name="body" value="/WEB-INF/views/settings/datasets/list.jspx"/>
        <put-attribute name="menu" value="/WEB-INF/views/settings/datasets/menu.jspx"/>
    </definition>
    
    <!-- 데이터집합 조회 -->
    <definition extends="default" name="settings/datasets/show" template="/WEB-INF/layouts/bootstrap.jspx">
		<put-attribute name="activeTab" value="dataset_list" cascade="true"/>
		<put-attribute name="pagemode" value="show" cascade="true"/>
		<put-attribute name="body" value="/WEB-INF/views/settings/datasets/show.jspx"/>
        <put-attribute name="menu" value="/WEB-INF/views/settings/datasets/menu.jspx"/>
    </definition>
    
    <!-- 데이터집합 업로드 -->
    <definition extends="default" name="settings/datasets/upload" template="/WEB-INF/layouts/bootstrap-blank.jspx">
		<put-attribute name="pagemode" value="import" cascade="true"/>
        <put-attribute name="body" value="/WEB-INF/views/settings/datasets/upload.jspx"/>
        <put-attribute name="menu" value="/WEB-INF/views/settings/datasets/menu.jspx"/>
    </definition>
    
    <!-- 데이터집합 처리완료 -->
    <definition extends="preview" name="settings/datasets/saved">
		<put-attribute name="pagemode" value="import" cascade="true"/>
        <put-attribute name="body" value="/WEB-INF/views/settings/datasets/saved.jspx"/>
    </definition>
    
    <!-- 데이터집합 수정 -->
	<definition extends="default" name="settings/datasets/update" template="/WEB-INF/layouts/bootstrap-blank.jspx">
		<put-attribute name="pagemode" value="update"  cascade="true"/>
		<put-attribute name="body" value="/WEB-INF/views/settings/datasets/update.jspx"/>
        <put-attribute name="menu" value="/WEB-INF/views/settings/datasets/menu.jspx"/>
    </definition>
	
	
	
</tiles-definitions>
