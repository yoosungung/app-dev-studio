function MainViewService(vue) {

    /**
     * 공지사항 목록 조회
     */
    this.readNoticeList = function() {
        CommonUtil.axios(vue)
        .get("readNoticeList.do", {})
        .then(function(response) {
            vue.noticeList = response.data;
        })
        ["catch"](function(error) {
            CommonUtil.showAxiosError(error);
        });
    };

    /**
     * Q&A 목록 조회
     */
    this.readQnaList = function() {
        CommonUtil.axios(vue)
        .get("readQnaList.do", {})
        .then(function(response) {
            vue.qnaList = response.data;
        })
        ["catch"](function(error) {
            CommonUtil.showAxiosError(error);
        });
    };

    /**
     * FAQ 목록 조회
     */
    this.readFaqList = function() {
        CommonUtil.axios(vue)
        .get("readFaqList.do", {})
        .then(function(response) {
            vue.faqList = response.data;
        })
        ["catch"](function(error) {
            CommonUtil.showAxiosError(error);
        });
    };

    /**
     * 사용중 배너 목록 조회
     */
    this.readBannerList = function() {
        CommonUtil.axios(vue)
        .get("readBannerList.do", {})
        .then(function(response) {
            $('#slides').empty();
            var bannerList = response.data;
            for (let i=0; i<bannerList.length; i++) {
                var banner = bannerList[i];
                CommonUtil.axios(vue)
                .post("readBannerFileList.do", banner)
                .then(function(fileResponse) {
                    console.log(fileResponse.data);
                    var filePath = CommonUtil.contextPath + '/common/file/download?dwldId=' + fileResponse.data[0].dwldId;
                    if (bannerList.length == 1) {
                        $('#slide_container').append('<img id=\"single_banner\" src=\"' + filePath + '\">');
                    }
                    else {
                        $('#slides').append('<div><img src=\"' + filePath + '\"></div>');
                        if (i == bannerList.length - 1 && i != 0) {
                            $('#single_banner').remove();
                            $('#slides').slidesjs({
                                width: 1294,
                                height: 364,
                                play: {
                                    active: true,
                                    auto: true,
                                    interval: 4000,
                                    swap: true
                                }
                            });
                        }
                    }
                })
                ["catch"](function(fileError) {
                    CommonUtil.showAxiosError(fileError);
                });
            }
        })
        ["catch"](function(error) {
            CommonUtil.showAxiosError(error);
        });
    };
}
