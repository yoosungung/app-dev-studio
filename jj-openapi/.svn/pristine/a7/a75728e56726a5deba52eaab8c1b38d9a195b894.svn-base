function BannerManageViewService(vue) {

    /**
     * 목록 조회
     */
    this.readList = function(firstPage) {
        alert("조회 테스트");
        CommonUtil.axios()
        .post("readList")
        .then(function(response) {
            vue.bannerList = response.data;
        })
        ["catch"](function(error) {
            GridUtil.showAxiosError(vue, error);
        });
    };

    /**
     * 추가
     */
    this.create = function() {
        if (!CommonUtil.checkValid()) {
            return;
        }
        if (!confirm("등록하시겠습니까?")) {
            return;
        }

        CommonUtil.axios()
        .post("create", vue.model)
        .then(function(response) {
            alert("등록되었습니다.");

            vues.list.service.readList();
        })
        ["catch"](function(error) {
            CommonUtil.showAxiosError(error);
        });
    };



}
