function BannerManageViewService(vue) {

    /**
     * 목록 조회
     */
    this.readList = function() {
        CommonUtil.axios()
        .post("readList",{})
        .then(function(response) {
            vue.bannerList = response.data;
        })
        ["catch"](function(error) {
            CommonUtil.showAxiosError(error);
        });
    };

    /**
     * 추가
     */
    this.create = function() {
        if (!CommonUtil.checkValid()) {
            return;
        }
        if (!confirm("등록하시겠습니까?")) {
            return;
        }

//        var formData = new FormData();
//        formData.put("sss제목", vue.model.ssss);
//        formData.put("file1", vue.model.ssss);

        CommonUtil.axios()
//        .post("create", formData)
        .post("create", vue.model)
        .then(function(response) {
            alert("등록되었습니다.");

            vues.list.service.readList();
        })
        ["catch"](function(error) {
            CommonUtil.showAxiosError(error);
        });
    };

    /**
     * 삭제
     */
    this.delete = function() {
        if (!confirm("삭제하시겠습니까?")) {
            return;
        }
        CommonUtil.axios()
        .post("delete", vue.bannerList)
        .then(function(response) {
            alert("삭제되었습니다.");
            vues.list.service.readList();
        })
        ["catch"](function(error) {
            CommonUtil.showAxiosError(error);
        });
    };

    /**
     * 파일 업로드
     */
    this.upload = function() {

        CommonUtil.axios()
        .post("upload", vue.file)
        .then(function(response) {
            alert("업로드되었습니다.");
        })
        ["catch"](function(error) {
            CommonUtil.showAxiosError(error);
        });
    }

    /**
     * 사용여부 업데이트
     */
    this.update = function(banner) {
        console.log(banner);
//        CommonUtil.axios()
//        .put("update", vue.bannerList)
//        .then(function(response) {
//            alert("삭제되었습니다.");
//            vues.list.service.readList();
//        })
//        ["catch"](function(error) {
//            CommonUtil.showAxiosError(error);
//        });
    };



}
