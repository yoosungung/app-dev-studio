package kr.ac.jj.shared.domain.main.model.com.person.user;

import kr.ac.jj.shared.domain.main.model.MainEntity;

/**
 * 공통 - 사람 사용자 Entity
 */
abstract class TbComPersonUserEntity extends MainEntity {

    private static final long serialVersionUID = 5718436576403846733L;

    protected String personUserId;
    protected String personId;
    protected String userId;

    public String getPersonUserId() {
        return this.personUserId;
    }

    public void setPersonUserId(String personUserId) {
        this.personUserId = personUserId;
    }

    public String getPersonId() {
        return this.personId;
    }

    public void setPersonId(String personId) {
        this.personId = personId;
    }

    public String getUserId() {
        return this.userId;
    }

    public void setUserId(String userId) {
        this.userId = userId;
    }

}
