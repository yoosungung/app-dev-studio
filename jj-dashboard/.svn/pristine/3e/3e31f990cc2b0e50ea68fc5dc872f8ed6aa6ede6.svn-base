<tiles-definitions>

    <definition name="tiles-shared:/**" template="/WEB-INF/tiles/dashboard/layout/TilesLayoutCommon.jsp">
        <put-attribute name="content" value="/WEB-INF/views/shared/{1}.jsp" />
    </definition>

    <definition name="tiles-common:/**" template="/WEB-INF/tiles/dashboard/layout/TilesLayoutCommon.jsp">
        <put-attribute name="content" value="/WEB-INF/views/dashboard/{1}.jsp" />
    </definition>

    <definition name="tiles-login:/**" template="/WEB-INF/tiles/dashboard/layout/TilesLayoutLogin.jsp">
        <put-attribute name="content" value="/WEB-INF/views/dashboard/{1}.jsp" />
    </definition>

    <definition name="tiles-main:/**" template="/WEB-INF/tiles/dashboard/layout/TilesLayoutMain.jsp">
        <put-attribute name="content" value="/WEB-INF/views/dashboard/{1}.jsp" />
    </definition>

</tiles-definitions>
