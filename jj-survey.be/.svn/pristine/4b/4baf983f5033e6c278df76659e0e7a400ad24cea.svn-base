package kr.ac.jj.survey.domain.main.model.sys.menu.author;

import kr.ac.jj.survey.domain.main.model.MainEntity;

/**
 * 시스템 - 메뉴별 권한 Entity
 */
abstract class TbSysMenuAuthorEntity extends MainEntity {
    private static final long serialVersionUID = 8747338431999748761L;

    protected String menuId;
    protected String authorId;

    public String getMenuId() {
        return this.menuId;
    }

    public void setMenuId(String menuId) {
        this.menuId = menuId;
    }

    public String getAuthorId() {
        return this.authorId;
    }

    public void setAuthorId(String authorId) {
        this.authorId = authorId;
    }
}
