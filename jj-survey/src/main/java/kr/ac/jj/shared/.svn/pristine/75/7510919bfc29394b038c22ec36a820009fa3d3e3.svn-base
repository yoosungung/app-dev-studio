package kr.ac.jj.shared.infrastructure.user;

import java.io.Serializable;

/**
 * 시스템 사용자
 */
public class SystemUser implements Serializable {

    private static final long serialVersionUID = -4467209928638543466L;

    public static final String USER_ID = "SYSTEM-USER-000000";
    public static final String PERSON_ID = "SYSTEM-PERSON-0000";

    public String getUserId() {
        return USER_ID;
    }

    public String getPersonId() {
        return PERSON_ID;
    }

}
