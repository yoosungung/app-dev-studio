@CHARSET "UTF-8";

#cloud_header { height:113px; position:relative; z-index:100; background:#fff; }
#cloud_header .header_content { position:relative; height:113px; width:980px; margin:0 auto; }
.wrap { min-height:100%; margin:-113px auto -85px auto; width:980px; background:url(/wcom/img/cloud/bg/right_bg.gif) repeat-y right top; }
* html .wrap { height:100%; }
#cloud_container { padding:113px 0 85px; }

#cloud_container #left_content  { position:relative; width:775px; min-height:785px; }
#cloud_container #right_content { position:absolute; margin-left:775px; width:203px; }

#cloud_footer { height:85px; background:#fff; }
#cloud_footer .footer_content{ position:relative; width:980px; height:82px; margin:0 auto; border-top:3px solid #343434; }

.visual { position:relative; width:775px; height:224px; overflow:hidden; }
.visual.default { background:url(/wcom/img/cloud/cloud_intro_bg.gif) left top no-repeat; }
.visual .organ_name { position:absolute; bottom:1px; right:0px; padding:10px 20px; color:#cdcdcd; font-size:25px; background:#000; opacity: .70;filter:Alpha(Opacity=70); line-height:36px; }
.visual .organ_name strong { font-weight:normal; color:#78a6f9; font-weight:bold; word-break:break-all; line-height:1; }
.contents_data { display:inline-block; width:745px; margin:40px 20px 30px 10px; }

.flogo { float:right; padding-top:23px; padding-right:10px; }
.fcopy { float:left; padding-left:10px; padding-top:23px; color:#626262; font-family:Dotum; }
.fcopy address { font-style:normal; }
.fcopy strong { color:#0471d6; }

.header_content .logo { float:left; padding-left:10px; padding-top:10px; height:63px; line-height:63px;}
.header_content .logo a img{vertical-align:middle}
.header_content .logo a.zeus_logo {}
.header_content .logo a.cloud_logo {margin-right:10px;  }
.header_content .logo a.cloud_logo { line-height:12px; }


/* 상단바로가기 STYLE
-------------------------------------------------*/
/* 화면 상단 링크 */
.header_content .tside_area { float:right; padding-top:45px; padding-right:10px; text-align:right; }
.header_content .tside_area .user   { padding-top:1px; text-align:right; }
.header_content .tside_area .user strong {  color:#399CE9;  }
.header_content .tside_area .user { float:left; margin-left:10px; text-align:right; }
.header_content .tsidemenu { float:left; margin-left:10px; }
.header_content .tsidemenu li { float:left; margin-left:10px; }
.header_content .tsidemenu li.login_form * { float:left; }
.header_content .tsidemenu li.login_form label { line-height:19px; margin-right:4px; }
.header_content .tsidemenu li.login_form input { margin-right:10px; width:90px; border:1px solid #e1e1e1; background:fff; }
.header_content .tsidemenu li.login_form button { margin:0; padding:0; border:0; background:none; width:61px; height:19px; }
.header_content .tsidemenu li a:hover, .header_content .tsidemenu li a:focus{ color:#4791de; }
.header_content .tsidemenu li.blue a { font-weight:bold; color:#fa7828;  }
/* 화면 상단 링크 // */

/* 상단 주 메뉴 */
#gnavigation { position:relative; float:left; width:978px; height:38px; border:solid 1px #142037; border-bottom:solid 1px #1e3052; border-top:solid 1px #8798b7; background:url(/wcom/img/cloud/bg/top_menu_bg.gif) repeat-x left top; }
#gnavigation a { display:block; color:#cee2ff; }
#gnavigation li { float:left; text-align:center; }
#gnavigation li ul.depth2 { border:0px; overflow:hidden; }
#gnavigation li ul.depth2 li { width:auto; border:0px; padding-left:20px; padding-right:0px; border-left:solid 1px #2E3F65; }
#gnavigation li ul.depth2 li.first { border:0px; }
#gnavigation li.menu01  { margin-right:42px;}
#gnavigation .depth2  { display:none; position:absolute; top:39px; left:-1px; width:775px; height:15px; padding:7px 0 8px 0; background:#5372a8; } 
#gnavigation .depth2 li {float:left; margin-right:20px; padding-right:20px; border-right:1px solid #2E3F65;white-space:nowrap;}
#gnavigation .depth2 li .last{margin-right:0; padding-right:0; border-right:0;}
#gnavigation .depth2 li.on a, #gnavigation .depth2 li a:hover, #gnavigation .depth2 li a:focus{ color:#fff; }
#gnavigation #subnav_list6  { width:768px; padding-left:7px; }
#gnavigation #subnav_list7  { width:647px; padding-left:128px; }
#gnavigation #subnav_list8  { width:513px; padding-left:262px;}
#gnavigation #subnav_list9  { width:378px; padding-left:397px;}
    /* 장비 검색 */
#gnavigation li.all_search_form { float:right; width:204px; z-index:2; padding-top:7px; }
#gnavigation li.all_search_form .inptxt {float:left; width:158px; padding:0 5px; border:solid 1px #141b29; height:21px; line-height:21px; }
#gnavigation li.all_search_form .btn {float:left; border:solid 1px #141b29; border-left:0px; }
/* 상단 주 메뉴 // */



/* 우측 달력 */
.calendar_schedule { position:relative; width:179px; padding:10px 13px ; background:#2c374d; }
.calendar_schedule h2 { position:absolute; left:-1px; top:-1px; height:1px; width:1px; overflow:hidden; }
.calendar { position:relative; width:179px; min-height:120px; margin-bottom:10px; overflow:hidden; }
.calendar_period { position:relative; height:24px; margin-bottom:8px; color:#fff; line-height:1; overflow:hidden; }
.calendar_period span { display:inline-block; float:left; }
.calendar_period span.month_control { line-height:21px;}
.calendar_period span.month_control strong  { margin:0 12px; font-size:21px; }
.calendar_period span.month_control button { border:0; margin:0; padding:0; background:0; color:#fff; font-size:16px; }
.calendar_period span.year { margin-right:32px; }
.calendar_period span.year,
.calendar_period span.eng_month { margin-top:12px; }
.calendar_period span.eng_month { float:right; margin-right:2px; }

.tb_cal_day {/* 달력 */ width:100%; font-size:0.95em; border-collapse:collapse; padding:0; margin:0; border:0; }
.tb_cal_day th { height:18px; width:26px; line-height:18px;  padding:0; background:#5f89c4; color:#fff; font-weight:normal; overflow:hidden; }
.tb_cal_day td { width:18px; height:12px; font-size:13px; line-height:12px; margin:0; padding:1px 8px 0 0; color:#7a8092; overflow:hidden; text-align:right; font-family:Arial, Helvetica, Verdana, AppleGothic, UnDotum, sans-serif; }
.tb_cal_day .sun { color:#ba2016; }
.tb_cal_day .sat { color:#39c; }  
.tb_cal_day a { float:right; color:#7a8092; }
.tb_cal_day .sun a { color:#ba2016; }
.tb_cal_day .sat a { color:#39c; }
.tb_cal_day a strong { margin-right:-4px; padding:3px 4px; background:#6e7485; color:#fff; } 
.tb_cal_day a { color:#7a8092; }
.tb_cal_day .sun a { color:#ba2016; }
.tb_cal_day .sat a { color:#39c; }    
.tb_cal_day a strong { margin-right:-4px; padding:3px 4px; background:#6e7485; color:#fff; }
.tb_cal_day td.today{ background:#6e7485; }
.tb_cal_day td.today a{ color:#fff; text-align:right; }

.schedule {display:block; min-height:40px;  padding:5px 0; background:#3e4a64;}
.schedule_list { width:165px;  padding:4px 7px 0;  color:#888fa3; font-size:0.9em; }
.schedule_list li { width:158px; padding-left:7px;  background:url(/wcom/img/icon/icon_dot.gif) no-repeat 0 7px; }

/* 우측 달력 // */

/* 우측 공지사항 */
.notice_area { position:relative; padding:10px;  border-bottom:1px solid #e1e1e1; }
.notice_area h2 { color:#252525; font-size:14px; }
.notice_area a.more { position:absolute; top:16px; right:10px; }
.notice_area .notice_list { list-style:none; padding-top:6px; }
.notice_area .notice_list li { list-style:none; background:url(/wcom/img/icon/icon_dot.gif) left 7px no-repeat; padding:0px; padding-left:10px; }
.notice_area .notice_list li a { color:#4c4c4c; font-size:12px; }
.notice_area .notice_list li a:hover, .notice_area .notice_list li a:focus{color:#4791de;}

/* 우측 공지사항 // */

/* 우측 기관 추천 장비 */
.recom_area { position:relative; padding:10px 10px 20px 10px; }
.recom_area h2 { color:#252525; font-size:14px; }
.recom_area button.left_btn { position:absolute; width:14px; height:13px; top:16px; right:23px; margin-top:6px;  padding:0px; border:0px; margin:0px; background:none; }
.recom_area button.right_btn { position:absolute; width:13px; height:13px; top:16px; right:10px; margin-top:6px;  padding:0px; border:0px; margin:0px; background:none; }
.recom_area .recom_list { list-style:none; position:relative; width:183px; overflow:hidden; }
.recom_area .recom_list .slider { position:relative; width:1000px; overflow-x:hidden; }
.recom_area .recom_list ul { list-style:none; position:relative; float:left; }
.recom_area .recom_list li { list-style:none; padding-top:6px; width:183px; }
.recom_area .recom_list li dt { list-style:none; background:url(/wcom/img/icon/icon_dot.gif) left 7px no-repeat; padding:0px; padding-left:10px; }
.recom_area .recom_list li dt a { color:#343434; font-size:12px; }
.recom_area .recom_list li dd { list-style:none; padding-left:10px; }
.recom_area .recom_list li dd a { color:#909090; font-size:11px; }
.recom_area .recom_list li dt a:hover,.recom_area .recom_list li dt a:focus{color:#4791de}

/* 우측 기관 추천 장비 // */

/* 화면 상단 타이틀 */
.contents_data .title_bar{position:relative; display:block;}
.contents_data h2{display:inline; color:#143f94; }
.contents_data h2 img{vertical-align:top;}
.contents_data h2.content_title { font-size:20px; color:#707070; display:block; position:relative; padding:0 10px 10px 0; margin-bottom:5px; margin-top:0px; }
.contents_data h2.content_title .stitle { font-size:12px; color:#909090; font-weight:normal}
.selection_options {display:inline;}
.other_guide{position:absolute; right:0; top:5px; text-align:right}

