/*조회버튼*/
a.cloud_search{
    padding:6px 11px 5px; font-weight:bold;border:1px solid #4e4e4e; color:#f7f7f7; display:inline-block;
    background:#515151 -webkit-gradient(linear, 0% 0%, 0% 100%, from(#717171), to(#515151));
    background:#515151 -moz-linear-gradient(top, #717171, #f515151);
    background:#515151 -o-linear-gradient(top, #717171, #515151);
    filter:progid:DXImageTransform.Microsoft.gradient(startColorStr=#717171, endColorStr=#515151);
    }
a.cloud_search:hover, a.cloud_search:focus{
    background:#515151 -webkit-gradient(linear, 0% 0%, 0% 100%, from(#515151), to(#717171));
    background:#515151 -moz-linear-gradient(top, #f515151, #717171);
    background:#515151 -o-linear-gradient(top, #f515151, #717171);
    filter:progid:DXImageTransform.Microsoft.gradient(startColorStr=#f515151, endColorStr=#717171);
    }
    
button.cloud_search{
    padding:6px 11px 5px; font-weight:bold;border:1px solid #4e4e4e; color:#f7f7f7; display:inline-block;
    background:#515151 -webkit-gradient(linear, 0% 0%, 0% 100%, from(#717171), to(#515151));
    background:#515151 -moz-linear-gradient(top, #717171, #f515151);
    background:#515151 -o-linear-gradient(top, #717171, #515151);
    filter:progid:DXImageTransform.Microsoft.gradient(startColorStr=#717171, endColorStr=#515151);
    }
button.cloud_search:hover, button.cloud_search:focus{
    background:#515151 -webkit-gradient(linear, 0% 0%, 0% 100%, from(#515151), to(#717171));
    background:#515151 -moz-linear-gradient(top, #f515151, #717171);
    background:#515151 -o-linear-gradient(top, #f515151, #717171);
    filter:progid:DXImageTransform.Microsoft.gradient(startColorStr=#f515151, endColorStr=#717171);
    }
    
