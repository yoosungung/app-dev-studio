/* 메인 장비 목록 */
ul.equip_list.cloud { position:relative; list-style:none; padding-top:10px; }
ul.equip_list.cloud li { position:relative; list-style:none; width:175px; height:180px; border:0px; padding:0px; margin:0px; margin-right:14px; }
ul.equip_list.cloud li.first {}
ul.equip_list.cloud li.last { margin-right:0px; }

ul.equip_list.cloud li .equip_detail { display:none; position:absolute; left:0px; top:0px; width:175px; height:115px; margin:0 auto; overflow:hidden; z-index:53; background:url(/wcom/img/bg/main_eq_name_bg.png) transparent; color:white; }
ul.equip_list.cloud li .equip_detail a { color:white; } 
ul.equip_list.cloud li .equip_detail ul { position:relative; list-style:none; height:auto; }
ul.equip_list.cloud li .equip_detail ul li { position:relative; list-style:none; padding-left:10px; width:160px; height:22px; word-break:break-all; margin:0 auto; }
ul.equip_list.cloud li .equip_detail ul li.eng_nm { font-weight:bold; border-bottom:1px solid #878687; height:39px; overflow:hidden; margin-bottom:6px;}
ul.equip_list.cloud li .equip_detail ul li.manufacture {height:18px}
ul.equip_list.cloud li .equip_detail ul li.model_nm {height:18px}
ul.equip_list.cloud li .equip_detail ul li.useType_nm {height:18px}

ul.equip_list.cloud li .equip_view { position:relative; width:100%; z-index:51; }
ul.equip_list.cloud li .equip_view .model_nm { position:absolute; z-index:52; top:89px; left:1px; width:163px; padding-left:10px; height:25px; line-height:25px; color:white; font-weight:bold; background:url(/wcom/img/bg/main_eq_name_bg.png) transparent; overflow:hidden; }
ul.equip_list.cloud li .equip_view .model_nm a { line-height:25px; color:white; font-weight:bold; }
ul.equip_list.cloud li .equip_view .img_frame { position:relative; z-index:51; width:173px; height:113px; border:1px solid #dddddd; overflow:hidden; }
ul.equip_list.cloud li .equip_view .recom_score { position:relative; font-weight:bold; color:#707070;  margin-top:5px;}
ul.equip_list.cloud li .equip_view .recom_score  img{vertical-align:middle;}
ul.equip_list.cloud li .equip_view .distance { position:absolute; right:0px; top:119px; color:#acacac; }
ul.equip_list.cloud li .equip_view .takeDt { position:absolute; right:0px; top:119px; color:#acacac; }
ul.equip_list.cloud li .equip_view .kor_nm { position:relative; color:#707070; width:175px; }
ul.equip_list.cloud li .equip_view .organ_nm { font-size:11px; }
ul.equip_list.cloud li .equip_view .organ_nm a { color:#909090; font-size:11px; }
