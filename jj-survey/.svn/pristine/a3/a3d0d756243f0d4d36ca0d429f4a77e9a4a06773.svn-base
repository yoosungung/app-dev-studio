@charset "utf-8";
/* CSS Document */

.skip{}
.skip a{position:absolute;top:-100px; left:0;display:block; margin:0;width:100%; padding:15px 0; text-align:center; background:#dbe7f6; z-index:9999;}
.skip a:hover, .skip a:active, .skip a:focus{top:0;}

.bnr_xmas{position:absolute; top:40px; right:10px; z-index:1000;}

#container{position:relative; width:100%; height:auto; }
#container .section_wrap{position:relative; width:1024px; min-height:707px;  margin:0 auto; zoom:1;}
#container .section_wrap:after{clear:both; display:block; content:""}

#container .section_wrap .sectiontit{padding:0px 15px; height:48px; line-height:48px; font-size:17px; font-weight:bold; font-family: "Nanum Gothic", "나눔고딕", "맑은고딕", "Malgun Gothic", "돋움", "Dotum";}
#container .section_wrap .sectiontit .newicon{display:inline-block; vertical-align:top; margin:18px 0px 0px 5px; width:36px; height:14px; background:url(../../wcom/img/zeus2014/main/sp_main.png) -300px -200px no-repeat; text-indent:-9999em; z-index:100;}
#container .section_wrap .divover:hover, #container .section_wrap .border:focus, #container .section_wrap .border:active{border:1px solid #00b7ce;}
.btn_arrow{display:block; position:absolute; top:103px; width:30px; height:30px; background:url(../../wcom/img/zeus2014/main/sp_main.png) no-repeat; z-index:10;}
.btn_arrow.prev{left:10px; background-position:-300px 0px;}
.btn_arrow.next{right:10px; background-position:-350px 0px;}
.bx-next{display:block; position:absolute; top:53px; width:30px; height:30px; right:-6px; background:url(../../wcom/img/zeus2014/main/sp_main.png) -350px 0px; no-repeat; z-index:51;}
.bx-prev{display:block; position:absolute; top:53px; width:30px; height:30px; left:-6px; background:url(../../wcom/img/zeus2014/main/sp_main.png) -300px 0px; no-repeat; z-index:51;}

