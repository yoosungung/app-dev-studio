.main_cont h4{margin:0 0 5px; font-weight:bold; font-family: "Nanum Gothic", "나눔고딕", "맑은고딕", "Malgun Gothic", "돋움", "Dotum"; font-size:14px;}
.main_cont h4 a{font-size:14px; color:#000;}
.main_cont .roll_wrap:after{clear:both; display:block; content:"";}
.main_cont .roll_box{float:left; margin:0 4px 0 0; width:24px; height:28px; background:url(../../wcom/img/zeus2014/main/bg_cont.png) no-repeat 0 0;}
