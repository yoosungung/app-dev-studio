.book_ranking{position:relative; float:left; width:100%; }
.tab_type1{position:relative; height:40px;}
.tab_type1:after{clear:both; display:block; content:""}
.tab_type1 li{float:left;}
.tab_type1 li a{display:block; margin-left:10px; padding-left:11px; line-height:40px; background:url(../../wcom/img/zeus2014/main/bullet_line.gif) left 15px no-repeat;}
.tab_type1 li a.first{margin:0px; padding:0px; background:none; }
.tab_type1 li.active a{font-weight:bold; color:#5d9ec9;}
.ranking_wrap{position:relative;}
.ranking_wrap:after{clear:both; display:block; content:""}
.ranking_wrap .ranking_list{position:relative; display:block; padding-left:254px;}
.ranking_wrap .ranking_list:after{clear:both; display:block; content:""}
.ranking_wrap .ranking_list li{position:relative; float:left; margin:0px 0px 24px 24px; width:122px; height:166px;}
.ranking_wrap .ranking_list li img{width:120px; height:166px; border:1px solid #e4e4e4;}

.ranking_wrap .ranking_list li.book_no1{display:block; position:absolute; top:0; left:0; margin:0px;}
.ranking_wrap .ranking_list li.book_no1 img{width:251px; height:356px; border:1px solid #e4e4e4;}

.ranking_wrap .ranking_list li .bookinfo{display:none; position:absolute; top:0px; left:0px; width:112px; height:128px; text-align:center; padding:40px 5px 0px 5px;  z-index:10;
	color:#fff; font-family:'Nanum Gothic'; background:url(../../wcom/img/zeus2014/main/black70.png);
	}
.ranking_wrap .ranking_list li .bookinfo .btn_detail{position:absolute; left:50%; bottom:30px; margin-left:-28px; width:56px !important; height:17px !important; border:none !important;}
.ranking_wrap .ranking_list li.book_no1 .bookinfo{width:243px; height:318px;}
.ranking_wrap .ranking_list li:hover .bookinfo, .ranking_wrap .ranking_list li:focus .bookinfo, .ranking_wrap .ranking_list li:active .bookinfo{display:block;}



/* FE인포 */
.info_newest{position:relative; float:left; padding-right:20px; width:218px; border-right:1px solid #e4e4e4;}
.info_newest a{display:block; position:relative;}
.info_newest a img{width:218px; height:356px;}
.info_newest a .blackline{display:block; position:absolute; top:0px; left:0px; width:217px; height:355px; border:1px solid #ccc; /*background:url(../../wcom/img/zeus2014/main/fe/info_line.png) no-repeat;*/ z-index:200;}


.info_pop{position:relative; float:left; padding-left:20px; width:725px;}
.info_pop .info_list{margin:-20px 0px 0px -20px;}
.info_pop .info_list:after{clear:both; display:block; content:""}
.info_pop .info_list li{position:relative; float:left; margin:20px 0px 0px 20px; width:129px; height:168px;}
.info_pop .info_list li img{width:129px; height:172px;}
.info_pop .info_list li .blackline{display:block; position:absolute; top:0px; left:0px; width:127px; height:170px; border:1px solid #ccc; z-index:200;}


/* FE노벨 */
.theme{position:relative; width:100%; overflow:hidden;}
.themelist{position:relative; width:1200px; height:140px; overflow:hidden; padding-bottom:20px; margin-bottom:5px; border-bottom:1px solid #e2e2e2;}
.themelist:after{clear:both; display:block; content:"";}
.themelist li{width:318px; float:left; margin-right:15px;}
.themelist li a{position:relative; display:block;}
.themelist li a img{width:100%; height:140px;}
.themelist li a .rec_bar{display:none;position:absolute; left:0; top:0; border:5px solid #00b7ce; width:308px; height:130px;}
.themelist li a:hover .rec_bar,
.themelist li a.active .rec_bar{display:block;}
.themelist .btn_pg{position:absolute; top:0; display:block; width:25px; height:8px; padding:66px 0; background:url(../../wcom/img/zeus2014/main/black70.png); text-align:center;}
.themelist .btn_pg.l_t{left:0;}
.themelist .btn_pg.r_t{left:959px;}

.winner{position:relative;}
.winner ul{width:1000px;}
.winner ul li{position:relative; float:left; margin-right:10px; width:112px; height:148px; border:1px solid #d9d9d9}
.winner ul li img{width:100%; height:148px;}
.winner ul li span{display:none; vertical-align:middle; position:absolute; top:0px; left:0px; text-align:center; padding:10px 5px 0px 5px; width:102px; height:138px; color:#fff; font-family:'Nanum Gothic'; background:url(../../wcom/img/zeus2014/main/black70.png);}
.winner ul li:hover span{display:block;}



/* FE대형 */
.fct{width:100%;}
.fct ul:after{clear:both; display:block; content:"";}
.fct li{position:relative; float:left; width:236px; height:173px; margin:0 0 10px 10px; border:1px solid #d9d9d9; background:#fff;}
.fct li.edge{margin-left:0;}
.fct li img{width:100%; height:173px;}
.fct li .thumb_title{position:absolute; left:0; bottom:0; width:216px; background:url(../../wcom/img/zeus2014/main/black70.png); color:#fff; padding:5px 10px; font-size:14px;overflow : hidden; white-space: nowrap; text-overflow:ellipsis; -o-text-overflow:ellipsis; -ms-text-overflow:ellipsis;}
.fct li .info{display:none; position:absolute; left:0; top:0; width:216px; height:158px; padding:15px 10px 0; background:url(../../wcom/img/zeus2014/main/black70.png); color:#fff;}
.fct li .info .title{font-size:14px; margin:0 0 10px;}
.fct li a:hover .info{display:block;}
.fct li a:hover .thumb_title{display:none;}



