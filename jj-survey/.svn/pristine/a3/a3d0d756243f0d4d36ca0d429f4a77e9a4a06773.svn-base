/* 공지사항 슬라이드 */
.notice{display:block; position:relative; width:1022px; margin:0 auto 20px; height:37px; line-height:37px; background:#fffef5; border:1px solid #e2e2e2; border-radius:4px;}
.notice h3{position:absolute; left:15px; top:2px; padding-left:10px; background:url(../../wcom/img/zeus2014/main/fe/sp_fe.png) -450px 15px no-repeat; z-index:52}
.notice h3 a{display:block;color:#333; height:35px; line-height:35px; font-size:14px; }
.notice .slide_wrap{position:relative; height:37px; line-height:37px; overflow:hidden;}
.notice .slide_list{position:absolute; left:100px;}
.notice .slide_list li{width:900px; height:39px; line-height:39px;}
.notice .slide_list li:after{clear:both; display:block; content:""}
.notice .slide_list li .icon_new{float:left; display:block; margin:12px 0 0 5px; vertical-align:top}
.notice .slide_list li a{float:left; display:block; max-width:800px;  height:39px; line-height:39px; overflow : hidden; white-space: nowrap; text-overflow:ellipsis; -o-text-overflow:ellipsis; -ms-text-overflow:ellipsis;}


.notice .bx-wrapper .bx-controls{position:absolute; right:15px; top:15px; z-index:60}
.notice .bx-wrapper .bx-controls a.bx-stop{display:inline-block; margin-right:4px; width:12px; height:12px; background:url(../../wcom/img/zeus2014/main/btn_nt_stop.png) no-repeat 0 0px;}
.notice .bx-wrapper .bx-controls a.bx-stop.active{background-position:0 -19px;}
.notice .bx-wrapper .bx-controls a.bx-start{display:inline-block; margin-right:4px; width:12px; height:12px; background:url(../../wcom/img/zeus2014/main/btn_nt_play.png) no-repeat 0 0px;}
.notice .bx-wrapper .bx-controls a.bx-start.active{background-position:0 -19px;}


/* 입력팁 */
.srch_tip{position:absolute; left:375px; top:-25px;}
.srch_tip dl{position:relative; padding-left:50px; font-family: "Nanum Gothic", "나눔고딕", "맑은고딕", "Malgun Gothic", "돋움", "Dotum";}
.srch_tip dl:after{clear:both; display:block; content:"";}
.srch_tip dt{position:absolute; left:0; top:0; padding:0 0 0 5px; width:45px; height:19px; line-height:19px; background:url(../../wcom/img/zeus2014/main/bg_tip.png) no-repeat 0 0; color:#777; }
.srch_tip dd{color:#999999; float:left; display:block; height:19px; line-height:19px; max-width:70px; margin:0 0 0 6px; padding:0 0 0 6px; background:url(../../wcom/img/zeus2014/main/bar_tip.gif) no-repeat 0 50%; overflow:hidden; white-space:nowrap; text-overflow:ellipsis; color:#777;}
.srch_tip dd.first{margin:0; padding:0; background:none;}
.srch_tip dd a{color:#999999;}
