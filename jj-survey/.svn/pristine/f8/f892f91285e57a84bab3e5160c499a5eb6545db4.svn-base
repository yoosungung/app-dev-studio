/*공통 컨텐츠 박스*/
.article{display:block; position:relative; float:left; width:383px; height:399px; background:#fff; border:1px solid #d4d5d6; zoom:1;}
.article:after{clear:both; display:block; content:""}
.article.subsize{width:488px;}
.article.mt{margin:10px 0px 0px 0px;}
.article.mtml{margin:10px 0px 0px 10px;}

.article_sub{display:block; position:relative; float:left; width:400px; height:399px; background:#fff; border:1px solid #d4d5d6; zoom:1;}
.article_sub:after{clear:both; display:block; content:""}

/*공통 리스트스타일_섬네일*/
.thumbstyle{position:relative; margin:18px 20px 0px 20px;}
.thumbstyle:after{clear:both; display:block; content:""}
.thumbstyle li{display:block; position:relative; float:left; margin:0 0 12px 9px; width:108px; height:100px;}
.thumbstyle li.first{margin:0px;}
.thumbstyle li p{display:block; text-align:center; padding-top:5px; width:108px;
overflow : hidden;
white-space: nowrap;
text-overflow:ellipsis;
-o-text-overflow:ellipsis;
-ms-text-overflow:ellipsis;
}
.thumbstyle li img{width:100%; height:80px;}
.thumbstyle li:hover p, .thumbstyle li:focus p, .thumbstyle li:active p{text-decoration:underline;}
.thumbstyle li .bkline{display:block; position:absolute; top:0px; left:0px; width:106px; height:78px; border:1px solid #000; opacity:0.2; filter:alpha(opacity=20);}
.thumbstyle.type2{}
.thumbstyle.type2 li{margin-left:9px; width:105px;}
.thumbstyle.type2 li.first{margin:0px;}
.thumbstyle.type2 li .bkline{width:103px;}


/*공통 리스트스타일_리스트*/
.liststyle{padding:15px 15px 0px 15px;}
.liststyle li{display:block; position:relative; padding-left:0px; height:24px; /* background:url(../../../wcom/img/zeus2014/share/main/bul_graydot.gif) 0px 7px no-repeat; */}
.liststyle li.ifreply{padding-left:18px; background:url(../../../wcom/img/zeus2014/share/main/bul_reply.gif) 9px 5px no-repeat;}
.liststyle li img{margin:3px 0 0 0;}
.liststyle li p{float:left; max-width:75%;
overflow : hidden;
white-space: nowrap;
text-overflow:ellipsis;
-o-text-overflow:ellipsis;
-ms-text-overflow:ellipsis;
margin: 0 3px 0 0;
}
.liststyle li p.lefttext{width:65%;}
.liststyle li .re{display:inline-block; margin-right:3px; width:16px; height:16px; line-height:16px; text-align:center; background:#eee; font-size:10px; font-family:"Arial"; color:#00b7ce; border-radius:20px;}
.liststyle li .category{padding-right:3px; color:#00b7ce;}
.liststyle li .date{float:right; width:35px; color:#888; text-align:right;}
.liststyle li .righttext{float:right; width:145px; color:#888;}
.liststyle li .righttext span{display:inline-block;}
.liststyle li .righttext .view{float:left; width:20px; text-align:center;}
.liststyle li .righttext .writer{float:left; padding:0px 10px; width:70px; text-align:center;
overflow : hidden;
white-space: nowrap;
text-overflow:ellipsis;
-o-text-overflow:ellipsis;
-ms-text-overflow:ellipsis;
}
.liststyle li a:hover p, .liststyle li a:focus p, .liststyle li a:active p{text-decoration:underline;}

/**/
.wrap_viewtype{position:absolute; top:10px; right:65px; font-size:0;}
.viewtype{display:inline-block; height:20px; background:url(../../../wcom/img/zeus2014/share/main/sp_viewtype.png) no-repeat; text-indent:-9999em;}
.viewtype.thumbtype{width:19px; background-position:0px 0px; margin-left:-1px; border-right:1px solid #ccc}
.viewtype.thumbtype:hover, .viewtype.thumbtype:focus, .viewtype.thumbtype:active, .viewtype.thumbtype.active{background-position:0px -50px;}
.viewtype.listtype{width:19px; background-position:0px -100px;}
.viewtype.listtype:hover, .viewtype.listtype:focus, .viewtype.listtype:active, .viewtype.listtype.active{background-position:0px -150px;}


/*컨텐츠*/
.mainbnr{display:block; position:relative; float:left; width:780px; height:148px;}

.mainflow{display:block; position:relative; width:198px; height:207px; background:#fafafa url(../../../wcom/img/zeus2014/share/main/bg_flow.png) no-repeat 0 0;border:1px solid #d4d5d6;}
.mainflow ol{position:relative; padding:12px 20px 0px 0; text-align:right;}
.mainflow ol li{font-size:14px; font-weight:bold; color:#00b7ce;}
.mainflow ol li a{font-size:14px; font-family:nanum gothic; font-weight:bold; color:#00b7ce;}  
.mainflow ol li.ed01{position:absolute; right:12px; top:22px;}
.mainflow ol li.ed02{position:absolute; right:12px; top:73px;}
.mainflow ol li.ed03{position:absolute; right:12px; top:122px;}
.mainflow ol li.ed04{position:absolute; right:12px; top:170px;}

.noticeofsale{display:block; position:relative; margin-top:10px; width:198px; height:279px; background:#e6e6e6 url(../../../wcom/img/zeus2014/share/main/bg_sale.png) 0px 48px no-repeat; border:1px solid #d4d5d6; zoom:1; overflow:hidden;}
