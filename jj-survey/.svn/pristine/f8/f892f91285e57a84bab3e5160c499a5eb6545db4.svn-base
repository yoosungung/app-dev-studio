.noticeofsale ul{position:relative; margin:130px 10px 0px 10px; width:158px; height:170px; overflow:hidden;}
.noticeofsale ul:after{clear:both; display:block; content:""}
.noticeofsale ul li{display:block; height:105px; width:178px; float:left; padding:0 10px 0 10px;}
.noticeofsale ul li.first{padding:0 10px 0 0;}
.noticeofsale .sale_con{position:relative;}
.noticeofsale .sale_con dt{padding:10px 0px 5px 0px; line-height:18px; text-align:center;}
.noticeofsale .sale_con dd{text-align:center}
.noticeofsale .sale_con .period{height:32px; line-height:32px; text-align:center; border-top:1px solid #000; border-bottom:1px solid #000;}
.noticeofsale .sale_con .location{font-size:11px; color:#0cbcc8; text-align:center;}

.donation{display:block; position:relative; margin-top:10px; width:198px; height:207px; background:#fafafa; border:1px solid #d4d5d6; zoom:1; overflow:hidden;}
.donation dl{position:relative; padding:10px;}
.donation dl dt{padding:10px 0px 7px 0px; font-weight:bold;
	overflow : hidden;
	white-space: nowrap;
	text-overflow:ellipsis;
	-o-text-overflow:ellipsis;
	-ms-text-overflow:ellipsis;
	}
.donation dl dd.thumb{position:absolute; top:10px; left:0px; width:100%; text-align:center;}
.donation dl dd.thumb img{width:140px; height:90px;}
.donation dl dd.thumb .bkline{display:block; position:absolute; top:0px; left:50%; margin-left:-70px; width:138px; height:88px; border:1px solid #000; opacity:0.2; filter:alpha(opacity=20);}
.donation dl dd.writer{padding-top:10px; text-align:right; font-size:11px; color:#888;}
.donation dl dd.writer span{padding-left:5px;}
.donation a:hover dl dt, .donation a:focus dl dt, .donation a:active dl dt{text-decoration:underline;}


.bnrbox{display:block; position:relative; margin-top:10px; width:200px;}





/*뭔지모름*/
.bx-default-pager {position:absolute; left:40px; top:22px;  list-style: none; z-index:10}
.bx-default-pager .bx-pager-item { float: left;margin: 0 1px;}
.bx-default-pager .bx-pager-item a {display:block; float:left; margin:1px; width:15px; height:14px; background:url(../../../wcom/img/zeus2014/common/sp_main.png) no-repeat -100px -200px; text-indent:-9999em; overflow: hidden;}
.bx-default-pager .bx-pager-item a.active,
.bx-default-pager .bx-pager-item a:hover.active {background-position: 0px -200px;}


/*커뮤니티 게시글 20150101 추가 */
#container .notice h2{background-position:20px -187px; background-color:#fbfbfb;}
.notice{display:block; position:relative; float:left; margin:10px 0px 0px 10px; /*width:383px; height:379px;*/width:363px; height:379px; /* background:#fff;  border:1px solid #d4d5d6;*/ zoom:1;}
.notice:after{clear:both; display:block; content:""}
.notice .noticelist{padding:0 10px;}
.notice .noticelist li{display:block; position:relative; padding:8px 0px; margin-bottom:4px; border-top:1px dashed #e4e4e4;}
.notice .noticelist li.first{border:none; padding-top:0px;}
.notice .noticelist li dl{margin:0px; padding:0px; font-size:0;}
.notice .noticelist li dl a{display:block; padding:0px; margin:0px;}
.notice .noticelist li dl dt{display:block;
}
.notice .noticelist li dl dt span{display:inline-block; vertical-align:middle; max-width:300px; height:21px;
overflow : hidden;
white-space: nowrap;
text-overflow:ellipsis;
-o-text-overflow:ellipsis;
-ms-text-overflow:ellipsis;
}
.notice .noticelist li dl dt img{padding:3px 0 0 3px; vertical-align:top;}
.notice .noticelist li dl dd{font-size:11px; color:#888;}
.notice .noticelist li dl dd span{display:inline-block;}
.notice .noticelist li dl dd.response{position:absolute; bottom:10px; right:0px;}
.notice .noticelist li dl dd.response span{padding:0px 5px 0px 20px; background:url(../../../wcom/img/zeus2014/common/sp_icon2.png) no-repeat}
.notice .noticelist li dl dd.response .view{background-position:0px 2px;}
.notice .noticelist li dl dd.response .likeit{background-position:0px -48px;}
.notice .noticelist li dl dd.response .reply{background-position:0px -98px;}
.notice .noticelist li dl dd .date{padding-left:10px; background:url(../../../wcom/img/zeus2014/common/sp_bullet.png) 5px -97px no-repeat}





