@charset "utf-8";

.skip{}
.skip a{position:absolute;top:-100px; left:0;display:block; margin:0; width:100%; padding:15px 0; text-align:center; background:#dbe7f6; z-index:9999;}
.skip a:hover, .skip a:active, .skip a:focus{top:0;}


/* 로그인  메인_서브 공통 */
.loginbox{display:block; position:relative; float:left; width:198px; height:118px; background:url(../../../wcom/img/zeus2014/common/bg_login.gif) left bottom no-repeat; border:1px solid #bcbcbc;}
.loginbox h2{display:none;}
.loginbox:after{clear:both; display:block; content:""}
.loginbox .loginmenu{margin:20px 25px 25px;}
.loginbox .loginmenu:after{clear:both; display:block; content:""}
.loginbox .loginmenu li a{display:block; background:url(../../../wcom/img/zeus2014/common/sp_login.png) no-repeat; text-indent:-9999px;}
.loginbox .loginmenu .btn_login{width:149px; height:31px; margin-bottom:10px; background-position:0 0;}
.loginbox .loginmenu .btn_join{width:149px; height:20px; background-position:0 -45px;}
.loginbox .loginmenu .btn_findid{float:left; width:45px; height:20px; background-position:-200px -45px;}
.loginbox .loginmenu .btn_findpw{float:left; width:66px; height:20px; background-position:-300px -45px;}
.loginbox .notice{margin:0px 21px;}
.loginbox .notice dt{display:block; height:19px; margin-bottom:10px; border-bottom:2px solid #000; background:url(../../../wcom/img/zeus2014/common/sp_login.png) 0px -100px no-repeat; text-indent:-9999px;}
.loginbox .notice dd{padding-left:15px; line-height:18px; background:url(../../../wcom/img/zeus2014/common/sp_bullet.png) 2px 3px no-repeat;}
.loginbox .notice dd a:hover, .loginbox .notice dd:focus, .loginbox .notice dd:active{text-decoration:underline;}
.loginbox .etcmenu{display:block; position:relative; width:158px; height:30px; margin:20px 21px 0px 21px; background:url(../../../wcom/img/zeus2014/common/sp_login.png) -200px -100px no-repeat;}
.loginbox .etcmenu li{float:left;}
.loginbox .etcmenu li a{display:block; width:79px; height:30px; text-indent:-9999px;}

.loginbox .myname{display:block; height:21px; padding:17px 20px 15px 20px; line-height:21px; text-align:center; }
.loginbox .btn_myzeus{display:block; position:absolute; top:17px; right:20px; width:59px; height:21px; background:url(../../../wcom/img/zeus2014/common/sp_login.png) 0px -150px no-repeat; text-indent:-9999px; z-index:10;}
.loginbox .btn_myzeus:hover, .loginbox .btn_myzeus:focus, .loginbox .btn_myzeus:active{background-position:-200px -150px;}
.loginbox .myinfo{margin-left:20px; width:156px;}
.loginbox .myinfo li{height:35px; line-height:35px; background:url(../../../wcom/img/zeus2014/common/sp_login.png) no-repeat;}
.loginbox .myinfo.subtype{padding-top:6px;}
.loginbox .myinfo.subtype li{height:25px; line-height:25px;}
.loginbox .myinfo li.info01{background-position:-290px -293px;}
.loginbox .myinfo li.info02{background-position:-290px -343px;}
.loginbox .myinfo li.info03{background-position:-290px -393px;}
.loginbox .myinfo li.info04{background-position:-290px -443px;}
.loginbox .myinfo li.info05{background-position:-290px -498px;}
.loginbox .myinfo li.info06{background-position:-290px -548px;}
.loginbox .myinfo li.info07{background-position:-290px -598px;}
.loginbox .myinfo li span{display:none;}
.loginbox .myinfo li strong{display:block; float:right; width:23px; text-align:center;}
.loginbox .myinfo li strong.tot{display:block; float:right; width:23px; text-align:center; font-weight: normal;}
.loginbox .myflow{display:block; position:absolute; left:0px; bottom:0px; height:60px; width:198px; height:60px; background:url(../../../wcom/img/zeus2014/common/sp_login.png) 0px -300px no-repeat;}
.loginbox .myflow li{width:33%; float:left; line-height:90px; text-align:center;}
.loginbox .myflow li span{display:none;}
.loginbox .myflow li a{line-height:90px;}

.loginbox .miniinfo{margin-left:15px; padding-top:8px;}
.loginbox .miniinfo li{height:23px; line-height:23px; padding-left:8px; font-size:11px; background:url(../../../wcom/img/zeus2014/common/sp_bullet.png) 0px -293px no-repeat;}
.loginbox .miniinfo li strong.minitit{padding-right:12px;}
.loginbox .miniinfo li strong.blue{color:#0cbcc8;}
.loginbox.sub{position:absolute; top:40px; right:0px; height:108px;}
