.loginbox.sub .loginmenu{margin:15px 0px 0px 25px;}
.loginbox.sub .myname{padding:14px 20px 12px 20px; border-bottom:1px solid #bcbcbc;} */




/* container */
#container{position:relative; width:100%; height:auto; margin:10px auto; zoom:1;}
#container:after{clear:both; display:block; content:""}
#section_wrap{display:block; position:relative; width:990px; margin:0 auto; zoom:1;}
#section_wrap:after{clear:both; display:block; content:"";}

#section_wrap .titlearea{display:block; height:40px; margin:10px 0 0; zoom:1;}
#section_wrap .titlearea:after{clear:both; display:block; content:"";}
#section_wrap .titlearea h2{float:left; line-height:40px; font-size:16px; font-family:'Nanum Gothic';}
#section_wrap .titlearea .menuguide{float:left; height:14px; line-height:14px; padding:15px 0px 0px 7px; font-size:11px;}


.content_wrap{display:block; position:relative;}
.content_wrap:after{clear:both; display:block; content:""}
.solution_wrap{display:block; position:relative;}
.solution_wrap:after{clear:both; display:block; content:""}
.board_wrap{display:block; position:relative; background:#fff;}
.board_wrap:after{clear:both; display:block; content:""}
.board_wrap2{display:block; position:relative; background:#fff; border:1px solid #e4e4e4;}
.board_wrap2:after{clear:both; display:block; content:""}
.blackwindow{display:block; position:absolute; top:0px; left:0px; width:100%; background:url(../../../wcom/img/zeus2014/common/bg_black.png); z-index:5000;}


.dtcon_wrap{display:block; position:relative; background:#fff url(../../../wcom/img/zeus2014/common/line_gray.gif) 789px top repeat-y; border:1px solid #e4e4e4;}
.dtcon_wrap:after{clear:both; display:block; content:""}
.dtcon_wrap .dtcon_left{display:block; position:relative; float:left; width:790px;}
.dtcon_wrap .dtcon_right{display:block; position:relative; float:right; padding:15px 19px; width:160px;}

.wrap_left{display:block; position:relative; float:left; width:780px; min-height:200px;}
.wrap_right{display:block; position:relative; float:right; width:200px;}

/* quick */
#quickmenu{display:block; position:fixed; top:0px; right:0px; width:3px; height:100%; background:#9699a2; border-left:1px solid #80848c; z-index:900;}
#quickmenu ul li{position:relative; background:url(../../../wcom/img/zeus2014/common/bg_quick.jpg) repeat-x;}
#quickmenu ul li a{display:block; padding-left:32px; height:43px; line-height:43px; font-size:11px; color:#fff; background:url(../../../wcom/img/zeus2014/common/sp_icon.png) no-repeat;}
#quickmenu .quick01{background-position:4px -137px;}
#quickmenu .quick02{background-position:4px -187px;}
#quickmenu .quick03{background-position:4px -237px;}
#quickmenu .quick04{background-position:4px -287px;}
#quickmenu .quick05{background-position:4px -337px;}
#quickmenu .mynum{display:block; position:absolute; top:6px; left:2px; width:15px; height:15px; text-align:center; font-size:11px; color:#fff; background:url(../../../wcom/img/zeus2014/common/sp_icon.png) 0px -400px no-repeat; z-index:1000;}
#quickmenu .quick_fold{display:block; position:fixed; top:50%; right:3px; width:13px; height:27px; background:url(../../../wcom/img/zeus2014/common/sp_btn.png) -100px -150px no-repeat; z-index:1000;}
#quickmenu .unfold{background-position:-200px -150px}

/* map 로고 위치 변경 */
.map_location_txt{width:85%; float:left;}
.logo_location{width:14%; position:absolute; float:right; right:0; bottom:13px;}
























/* market cafe */
#container .section_wrap{position:relative; width:100%; margin:10px 0 0; padding:0 0 20px; zoom:1}
#container .section_wrap:after{clear:both; display:block; content:""}
#container .section_left{display:block; position:relative; float:left; zoom:1}
#container .section_left:after{clear:both; display:block; content:""}
#container .section_right{display:block; position:relative; float:right; zoom:1}
#container .section_right:after{clear:both; display:block; content:""}

#container .section_wrap.cafe{width:990px; margin:10px auto;}
#container .section_wrap.cafe .section_left{width:700px;}
#container .section_wrap.cafe .section_right{width:280px;}
#container .section_wrap.cafe.sub{margin-bottom:0; width:990px; margin: -5px auto; padding:0 0 10px 0;/* background:url(../../wcom/img/zeus2014/common/bg_aside3.gif) repeat-y 0 0; */}
#container .section_wrap.cafe.sub.bgn{background:none}
#container .section_wrap.cafe.sub .section_left{border-top:1px solid #dbdbdb}
#container .section_wrap.cafe.sub.bgn .section_left{border-top:0;}
#container .section_wrap.cafe.sub .aside{display:block; position:relative; float:left; padding:0 0 0 0; width:144px;}
#container .section_wrap.cafe.sub .content{display:block; position:relative; float:right; width:545px; padding:0 0;}
