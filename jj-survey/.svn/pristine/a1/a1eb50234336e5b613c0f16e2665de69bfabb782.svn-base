<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jsp:root xmlns:c="http://java.sun.com/jsp/jstl/core"
    xmlns:fn="http://java.sun.com/jsp/jstl/functions"
    xmlns:util="urn:jsptagdir:/WEB-INF/tags/jd/util"
    xmlns:spring="http://www.springframework.org/tags"
    xmlns:fmt="http://java.sun.com/jsp/jstl/fmt"
    xmlns:jsp="http://java.sun.com/JSP/Page"
    xmlns:form="http://www.springframework.org/tags/form"
    version="2.0">

    <jsp:directive.tag import="java.util.ArrayList" />
    <jsp:output omit-xml-declaration="yes" />
    <jsp:directive.attribute name="questions" type="java.util.Collection" required="true" rtexprvalue="true" description="The collection to be displayed in the table" />
    <jsp:directive.attribute name="pageid" type="java.lang.Long" required="true" rtexprvalue="true" description="The identifier of the question page" />
    <jsp:directive.attribute name="pageorder" type="java.lang.Short" required="true" rtexprvalue="true" description="The identifier of the question page" />
    <jsp:directive.attribute name="surveyStatus" type="java.lang.String" required="true" rtexprvalue="true" description="The status of the survey definition: I Incomplete P published D Inactive " />

    <script type="text/javascript">
        function delSurveyPageQuestion(id) {
            if(confirm('질문을 삭제 하시겠습니까?')) {
                $('#' + id).submit();
            }
        }
    </script>

    <c:if test="${empty questions}">
        <div class="alert alert-warning fade in radius-bordered alert-shadowed">
            <i class="fa-fw fa fa-warning"><!----></i>
            <strong>경고</strong> 등록된 질문이 없습니다. 질문을 등록하세요.
        </div>
    </c:if>

    <section>
           <c:forEach items="${questions}" var="question" varStatus="loop">

            <c:if test="${loop.index > 0}">
                <hr class="wide" />
            </c:if>

            <div class="invoice-notes row">
                <div class="col-xs-10">
                    <div class="col-xs-1" style="width: 40px">
                        <span class="badge badge-sky badge-square">${question.order}</span>
                    </div>
                    <div class="col-sm-11" style="width: 970px">
                        <div class="form-group" style="margin-bottom: 0;">
                            <label for="questionAnswers${loop.index}">
                                <p class="pull-left">${question.questionText}</p>
                                <c:if test="${question.required}">
                                    <span class="pull-left label label-darkorange graded" style="margin-left: 5px;">필수</span>
                                </c:if>
                                <c:if test="${question.duplicate}">
                                    <span class="pull-left label label-success graded" style="margin-left: 5px;">중복</span>
                                </c:if>
                            </label>

                            <c:choose>
                                   <!-- BR    'Yes No DropDown'-->
                                <c:when test='${question.type.code == "BR"}'>
                                    <div class="form-inline">
                                        <select path="" class="form-control">
                                            <option value="false" selected="selected">예</option>
                                            <option value="true" >아니오</option>
                                        </select>
                                    </div>
                                </c:when>

                                <!-- ST    'Short Text Input'-->
                                <c:when test='${question.type.code == "ST"}'>
                                    <div class="form-inline">
                                        <input name="q" type="text" class="form-control" />
                                    </div>
                                </c:when>

                                <!-- LT    'Long Text Input'-->
                                <c:when test='${question.type.code == "LT"}'>
                                    <input name="q" type="text" class="form-control" />
                                </c:when>

                                <!-- HT    'Huge Text Input'-->
                                <c:when test='${question.type.code == "HT"}'>
                                    <textarea name="q" class="form-control"><!----></textarea>
                                </c:when>

                                <!-- IN    'Integer Input'-->
                                <c:when test='${question.type.code == "IN"}'>
                                    <div class="form-inline">
                                        <input name="q" type="text" class="form-control" />
                                    </div>
                                </c:when>

                                <!-- NM    'Numeric Input' -->
                                <c:when test='${question.type.code == "NM"}'>
                                    <div class="form-inline">
                                        <input name="q" type="text" class="form-control" />
                                    </div>
                                </c:when>

                                <!-- CR    'Currency Input'-->
                                <c:when test='${question.type.code == "CR"}'>
                                    <div class="form-inline">
                                        <input name="q" type="text" class="form-control" />
                                    </div>
                                </c:when>

                                <!-- DT    'Date Input' -->
                                <c:when test='${question.type.code == "DT"}'>
                                    <div class="form-inline">
                                        <div class="input-group">
                                            <input name="q" type="text" class="form-control" />
                                            <span class="input-group-addon">
                                                <i class="fa fa-calendar"><!----></i>
                                            </span>
                                        </div>
                                    </div>
                                </c:when>

                                <!-- SD 'Single choice Drop Down'-->
                                <c:when test='${question.type.code == "SD"}'>
                                    <c:choose>
                                        <c:when test="${empty question.options}">
                                            <div class="col-xs-12 alert alert-warning fade in radius-bordered alert-shadowed">
                                                <i class="fa-fw fa fa-warning"><!----></i>
                                                <strong>경고</strong> 등록된 옵션이 없습니다. 옵션을 등록하세요.
                                            </div>
                                        </c:when>
                                        <c:otherwise>
                                            <div class="form-inline">
                                                <select class="form-control">
                                                    <option value="">:: 선택 ::</option>
                                                    <c:forEach items="${question.options}" var="option" varStatus="loop2">
                                                          <option value="${option.value}"><c:out value="${option.text}"/></option>
                                                      </c:forEach>
                                                </select>
                                            </div>
                                        </c:otherwise>
                                    </c:choose>
                                </c:when>

                                <!-- SR 'Single Choice Radio'-->
                                <c:when test='${question.type.code == "SR"}'>
                                    <c:choose>
                                        <c:when test="${empty question.options}">
                                            <div class="col-xs-12 alert alert-warning fade in radius-bordered alert-shadowed">
                                                <i class="fa-fw fa fa-warning"><!----></i>
                                                <strong>경고</strong> 등록된 옵션이 없습니다. 옵션을 등록하세요.
                                            </div>
                                        </c:when>
                                        <c:otherwise>
                                            <c:choose>
                                                <!-- 수평 -->
                                                <c:when test='${question.direction == "HORIZONTAL"}'>
                                                    <div class="form-inline">
                                                        <c:forEach items="${question.options}" var="option" varStatus="loop">
                                                            <div class="radio">
                                                                <label>
                                                                    <input type="radio" name="question" value="option.value" />
                                                                    <span class="text">${option.text}</span>
                                                                </label>
                                                            </div>
                                                        </c:forEach>
                                                        <c:if test="${question.allowOtherTextBox}">
                                                            기타 : <input type="text" class="form-control input-sm" />
                                                        </c:if>
                                                    </div>
                                                </c:when>
                                                <!-- 수직 -->
                                                <c:otherwise>
                                                    <c:forEach items="${question.options}" var="option" varStatus="loop">
                                                        <div class="checkbox">
                                                            <label>
                                                                <input type="radio" name="question" value="option.value" />
                                                                <span class="text">${option.text}</span>
                                                            </label>
                                                        </div>
                                                    </c:forEach>
                                                    <c:if test="${question.allowOtherTextBox}">
                                                        <p class="form-inline">
                                                            기타 : <input type="text" class="form-control input-sm" />
                                                        </p>
                                                    </c:if>
                                                </c:otherwise>
                                            </c:choose>
                                        </c:otherwise>
                                    </c:choose>
                                </c:when>

                                <!-- MC 'Multiple Choice Checkboxes'-->
                                <c:when test='${question.type.code == "MC"}'>
                                    <c:choose>
                                        <c:when test="${empty question.options}">
                                            <div class="col-xs-12 alert alert-warning fade in radius-bordered alert-shadowed">
                                                <i class="fa-fw fa fa-warning"><!----></i>
                                                <strong>경고</strong> 등록된 옵션이 없습니다. 옵션을 등록하세요.
                                            </div>
                                        </c:when>
                                        <c:otherwise>
                                            <c:choose>
                                                <!-- 수평 -->
                                                <c:when test='${question.direction == "HORIZONTAL"}'>
                                                    <form role="form" class="form-inline">
                                                        <c:forEach items="${question.options}" var="option" varStatus="loop">
                                                            <div class="checkbox">
                                                                <label>
                                                                    <input type="checkbox" name="question" value="option.value" />
                                                                    <span class="text">${option.text}</span>
                                                                </label>
                                                            </div>
                                                        </c:forEach>
                                                        <c:if test="${question.allowOtherTextBox}">
                                                            기타 : <input type="text" class="form-control input-sm" />
                                                        </c:if>
                                                    </form>
                                                </c:when>
                                                <!-- 수직 -->
                                                <c:otherwise>
                                                    <c:forEach items="${question.options}" var="option" varStatus="loop">
                                                        <div class="checkbox">
                                                            <label>
                                                                <input type="checkbox" name="question" value="option.value" />
                                                                <span class="text">${option.text}</span>
                                                            </label>
                                                        </div>
                                                    </c:forEach>
                                                    <c:if test="${question.allowOtherTextBox}">
                                                        <p class="form-inline">
                                                            기타 : <input type="text" class="form-control input-sm" />
                                                        </p>
                                                    </c:if>
                                                </c:otherwise>
                                            </c:choose>
                                        </c:otherwise>
                                    </c:choose>
                                </c:when>

                                <!-- BRM: Yes No DropDown Matrix-->
                                <c:when test='${question.type.code == "BRM"}'>
                                    <c:choose>
                                        <c:when test="${empty question.rowLabels || empty question.columnLabels}">
                                            <div class="col-xs-12 alert alert-warning fade in radius-bordered alert-shadowed">
                                                <i class="fa-fw fa fa-warning"><!----></i>
                                                <strong>경고</strong> 행/열에 대한 정의가 필요합니다. 행/열을 등록하세요.
                                            </div>
                                        </c:when>
                                        <c:otherwise>
                                            <table class="table table-bordered table-hover">
                                                <thead>
                                                    <tr>
                                                        <th><!----></th>
                                                        <c:forEach items="${question.columnLabels}" var="col">
                                                            <th class="text-align-center"><c:out value="${col.label}"/></th>
                                                        </c:forEach>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <c:forEach var="row" items="${question.rowLabels}" varStatus="rindex">
                                                        <tr>
                                                            <th class="text-align-center"><c:out value="${row.label}"/></th>
                                                            <c:forEach items="${question.columnLabels}" var="col" varStatus="cindex">
                                                                <td>
                                                                    <select path="questionAnswers[${loop.index}].booleanAnswerValuesMatrix[${rindex.index}][${cindex.index}]">
                                                                        <option value="false" selected="selected">예</option>
                                                                        <option value="true" >아니오</option>
                                                                    </select>
                                                                </td>
                                                            </c:forEach>
                                                        </tr>
                                                    </c:forEach>
                                                </tbody>
                                            </table>
                                        </c:otherwise>
                                    </c:choose>
                                </c:when>

                                <!-- STM: Short Text Input Matrix-->
                                <c:when test='${question.type.code == "STM"}'>
                                    <c:choose>
                                        <c:when test="${empty question.rowLabels || empty question.columnLabels}">
                                            <div class="col-xs-12 alert alert-warning fade in radius-bordered alert-shadowed">
                                                <i class="fa-fw fa fa-warning"><!----></i>
                                                <strong>경고</strong> 행/열에 대한 정의가 필요합니다. 행/열을 등록하세요.
                                            </div>
                                        </c:when>
                                        <c:otherwise>
                                            <table class="table table-bordered table-hover">
                                                <thead>
                                                    <tr>
                                                        <th><!----></th>
                                                        <c:forEach items="${question.columnLabels}" var="col">
                                                            <th class="text-align-center"><c:out value="${col.label}"/></th>
                                                        </c:forEach>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <c:forEach items="${question.rowLabels}" var="row" varStatus="rindex">
                                                        <tr>
                                                            <th class="text-align-center"><c:out value="${row.label}"/></th>
                                                            <c:forEach items="${question.columnLabels}" var="col" varStatus="cindex">
                                                                <td>
                                                                    <input path="questionAnswers[${loop.index}].stringAnswerValuesMatrix[${rindex.index}][${cindex.index}]" type="text" maxlength="50" size="20" class="form-control input-sm" />
                                                                </td>
                                                            </c:forEach>
                                                        </tr>
                                                    </c:forEach>
                                                </tbody>
                                            </table>
                                        </c:otherwise>
                                    </c:choose>
                                </c:when>

                                <!-- INM: Integer Input Matrix-->
                                <c:when test='${question.type.code == "INM"}'>
                                    <c:choose>
                                        <c:when test="${empty question.rowLabels || empty question.columnLabels}">
                                            <div class="col-xs-12 alert alert-warning fade in radius-bordered alert-shadowed">
                                                <i class="fa-fw fa fa-warning"><!----></i>
                                                <strong>경고</strong> 행/열에 대한 정의가 필요합니다. 행/열을 등록하세요.
                                            </div>
                                        </c:when>
                                        <c:otherwise>
                                            <table class="table table-bordered table-hover">
                                                <thead>
                                                    <tr>
                                                        <th><!----></th>
                                                        <c:forEach items="${question.columnLabels}" var="col">
                                                            <th class="text-align-center"><c:out value="${col.label}"/></th>
                                                        </c:forEach>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <c:forEach items="${question.rowLabels}" var="row" varStatus="rindex">
                                                        <tr>
                                                            <th class="text-align-center"><c:out value="${row.label}"/></th>
                                                            <c:forEach items="${question.columnLabels}" var="col" varStatus="cindex">
                                                                <td>
                                                                    <input path="questionAnswers[${loop.index}].stringAnswerValuesMatrix[${rindex.index}][${cindex.index}]" type="text" maxlength="35" class="form-control input-sm"/>
                                                                </td>
                                                            </c:forEach>
                                                        </tr>
                                                    </c:forEach>
                                                </tbody>
                                            </table>
                                        </c:otherwise>
                                    </c:choose>
                                </c:when>

                                <!-- NMM: Decimal Input Matrix-->
                                <c:when test='${question.type.code == "NMM"}'>
                                    <c:choose>
                                        <c:when test="${empty question.rowLabels || empty question.columnLabels}">
                                            <div class="col-xs-12 alert alert-warning fade in radius-bordered alert-shadowed">
                                                <i class="fa-fw fa fa-warning"><!----></i>
                                                <strong>경고</strong> 행/열에 대한 정의가 필요합니다. 행/열을 등록하세요.
                                            </div>
                                        </c:when>
                                        <c:otherwise>
                                            <table class="table table-bordered table-hover">
                                                <thead>
                                                    <tr>
                                                        <th><!----></th>
                                                        <c:forEach items="${question.columnLabels}" var="col">
                                                            <th class="text-align-center"><c:out value="${col.label}"/></th>
                                                        </c:forEach>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <c:forEach items="${question.rowLabels}" var="row" varStatus="rindex">
                                                        <tr>
                                                            <th class="text-align-center"><c:out value="${row.label}"/></th>
                                                            <c:forEach items="${question.columnLabels}" var="col" varStatus="cindex">
                                                                <td>
                                                                    <input path="questionAnswers[${loop.index}].stringAnswerValuesMatrix[${rindex.index}][${cindex.index}]" type="text" maxlength="50" class="form-control input-sm"/>
                                                                </td>
                                                            </c:forEach>
                                                        </tr>
                                                    </c:forEach>
                                                </tbody>
                                            </table>
                                        </c:otherwise>
                                    </c:choose>
                                </c:when>

                                <!-- CRM: Currency Input Matrix-->
                                <c:when test='${question.type.code == "CRM"}'>
                                    <c:choose>
                                        <c:when test="${empty question.rowLabels || empty question.columnLabels}">
                                            <div class="col-xs-12 alert alert-warning fade in radius-bordered alert-shadowed">
                                                <i class="fa-fw fa fa-warning"><!----></i>
                                                <strong>경고</strong> 행/열에 대한 정의가 필요합니다. 행/열을 등록하세요.
                                            </div>
                                        </c:when>
                                        <c:otherwise>
                                            <table class="table table-bordered table-hover">
                                                <thead>
                                                    <tr>
                                                        <th><!----></th>
                                                        <c:forEach items="${question.columnLabels}" var="col">
                                                            <th class="text-align-center"><c:out value="${col.label}"/></th>
                                                        </c:forEach>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <c:forEach items="${question.rowLabels}" var="row" varStatus="rindex">
                                                        <tr>
                                                            <th class="text-align-center"><c:out value="${row.label}"/></th>
                                                            <c:forEach items="${question.columnLabels}" var="col" varStatus="cindex">
                                                                <td>
                                                                    <input path="questionAnswers[${loop.index}].stringAnswerValuesMatrix[${rindex.index}][${cindex.index}]" type="text" maxlength="50" class="form-control input-sm"/>
                                                                </td>
                                                            </c:forEach>
                                                        </tr>
                                                    </c:forEach>
                                                </tbody>
                                            </table>
                                        </c:otherwise>
                                    </c:choose>
                                </c:when>

                                <!-- DTM: Date Input Matrix-->
                                <c:when test='${question.type.code == "DTM"}'>
                                    <c:choose>
                                        <c:when test="${empty question.rowLabels || empty question.columnLabels}">
                                            <div class="col-xs-12 alert alert-warning fade in radius-bordered alert-shadowed">
                                                <i class="fa-fw fa fa-warning"><!----></i>
                                                <strong>경고</strong> 행/열에 대한 정의가 필요합니다. 행/열을 등록하세요.
                                            </div>
                                        </c:when>
                                        <c:otherwise>
                                            <table class="table table-bordered table-hover">
                                                <thead>
                                                    <tr>
                                                        <th><!----></th>
                                                        <c:forEach items="${question.columnLabels}" var="col">
                                                            <th class="text-align-center"><c:out value="${col.label}"/></th>
                                                        </c:forEach>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <c:forEach items="${question.rowLabels}" var="row" varStatus="rindex">
                                                        <tr>
                                                            <th class="text-align-center"><c:out value="${row.label}"/></th>
                                                            <c:forEach items="${question.columnLabels}" var="col" varStatus="cindex">
                                                                <td>
                                                                    <div class="form-inline">
                                                                        <div class="input-group">
                                                                            <input path="questionAnswers[${loop.index}].stringAnswerValuesMatrix[${rindex.index}][${cindex.index}]" type="text" maxlength="50" class="form-control input-sm"/>
                                                                            <span class="input-group-addon">
                                                                                <i class="fa fa-calendar"><!----></i>
                                                                            </span>
                                                                        </div>
                                                                    </div>
                                                                </td>
                                                            </c:forEach>
                                                        </tr>
                                                    </c:forEach>
                                                </tbody>
                                            </table>
                                        </c:otherwise>
                                    </c:choose>
                                </c:when>

                                <!-- CPM: Complex Input Matrix-->
                                <c:when test='${question.type.code == "CPM"}'>
                                    <c:choose>
                                        <c:when test="${empty question.rowLabels || empty question.columnLabels}">
                                            <div class="col-xs-12 alert alert-warning fade in radius-bordered alert-shadowed">
                                                <i class="fa-fw fa fa-warning"><!----></i>
                                                <strong>경고</strong> 행/열에 대한 정의가 필요합니다. 행/열을 등록하세요.
                                            </div>
                                        </c:when>
                                        <c:otherwise>
                                            <table class="table table-bordered table-hover">
                                                <thead>
                                                    <tr>
                                                        <th><!----></th>
                                                        <c:forEach items="${question.columnLabels}" var="col">
                                                            <th class="text-align-center"><c:out value="${col.label}"/></th>
                                                        </c:forEach>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <c:forEach items="${question.rowLabels}" var="row" varStatus="rindex">
                                                        <tr>
                                                            <th class="text-align-center"><c:out value="${row.label}"/></th>
                                                            <c:forEach items="${question.columnLabels}" var="col" varStatus="cindex">
                                                                <td>
                                                                    <input path="questionAnswers[${loop.index}].stringAnswerValuesMatrix[${rindex.index}][${cindex.index}]" type="text" maxlength="50" size="20" class="form-control input-sm" />
                                                                </td>
                                                            </c:forEach>
                                                        </tr>
                                                    </c:forEach>
                                                </tbody>
                                            </table>
                                        </c:otherwise>
                                    </c:choose>
                                </c:when>

                                <!-- FIL 'File Upload'-->
                                <c:when test='${question.type.code == "FIL"}'>
                                    <input type="file" name="file${question.id}" id="file${question.id}" size="50" class="form-control"/>
                                </c:when>
                            </c:choose>
                            <c:if test="${not empty question.tip}">
                                <p class="help-block" style="margin-bottom: 0px;">${question.tip}</p>
                            </c:if>
                        </div>

                    </div>
                </div>

                <div class="text-right col-xs-2" style="margin: 0; padding: 0 50px 0 0; position: absolute; text-align: right; left: 1019px; width: 300px;">

                    <!-- Update link Allow only when survey status is equal to 'I' Incomplete -->
                    <c:if test="${surveyStatus eq 'I'}">
                        <spring:url value="/settings/questions/${question.id}" var="update_form_url">
                            <spring:param name="form" />
                        </spring:url>
                        <a href="${update_form_url}" class="fancyboxLink tooltip-info" target="_blank" data-original-title="질문 수정" data-toggle="tooltip" data-placement="bottom" style="margin: 0 5px; font-size: 20px;">
                            <i class="fa fa-cog" style="font-size: 20px;"><!----></i>
                        </a>
                    </c:if>

                    <!-- Copy link Allow only when survey status is equal to 'I' Incomplete -->
                    <c:if test="${surveyStatus eq 'I'}">
                        <spring:url value="/settings/questions/${question.id}" var="copy_form_url">
                            <spring:param name="copy" />
                        </spring:url>
                        <a href="${copy_form_url}" class="fancyboxLink tooltip-info" target="_blank" data-original-title="질문 복사" data-toggle="tooltip" data-placement="bottom" style="margin: 0 5px; font-size: 20px;">
                            <i class="fa fa-copy" style="font-size: 20px;"><!----></i>
                        </a>
                    </c:if>

                    <!-- Update options link Allow only when survey surveyStatus is equal to 'I' Incomplete -->
                    <c:if test="${question.type.requiresOptions and surveyStatus eq 'I'}" >
                        <spring:url value="/settings/questionOptions/${question.id}" var="update_form_url">
                            <spring:param name="form" />
                        </spring:url>
                        <a href="${update_form_url}" class="fancyboxLink tooltip-info" target="_blank" data-original-title="옵션 수정" data-toggle="tooltip" data-placement="bottom" style="margin: 0 5px; font-size: 20px;">
                            <i class="fa fa-edit" style="font-size: 20px;"><!----></i>
                        </a>
                    </c:if>

                    <!-- Update columns link Allow only when survey surveyStatus is equal to 'I' Incomplete -->
                    <c:if test="${question.type.isMatrix and  surveyStatus eq 'I'}" >
                        <spring:url value="/settings/questionCols/${question.id}" var="update_form_url">
                            <spring:param name="form" />
                        </spring:url>
                        <a href="${update_form_url}" class="fancyboxLink tooltip-info" target="_blank" data-original-title="행 수정" data-toggle="tooltip" data-placement="bottom" style="margin: 0 5px; font-size: 20px;">
                            <i class="fa fa-arrows-h" style="font-size: 20px;"><!----></i>
                        </a>
                    </c:if>

                    <!-- Update rows link Allow only when survey surveyStatus is equal to 'I' Incomplete -->
                    <c:if test="${question.type.isMatrix and  surveyStatus eq 'I'}" >
                        <spring:url value="/settings/questionRows/${question.id}" var="update_form_url">
                            <spring:param name="form" />
                        </spring:url>
                        <a href="${update_form_url}"  class="fancyboxLink tooltip-info" target="_blank" data-original-title="열 수정" data-toggle="tooltip" data-placement="bottom" style="margin: 0 5px; font-size: 20px;">
                            <i class="fa fa-arrows-v" style="font-size: 20px;"><!----></i>
                        </a>
                    </c:if>

                    <!-- Delete link Allow only when survey surveyStatus is equal to 'I' Incomplete -->
                    <c:if test="${surveyStatus eq 'I'}">
                        <a href="javascript:delSurveyPageQuestion('${question.type.code}_id_${question.id}')" class="tooltip-danger" data-original-title="질문 삭제" data-toggle="tooltip" data-placement="bottom" style="margin: 0 5px; font-size: 20px;">
                            <i class="fa fa-trash-o" style="font-size: 20px;"><!----></i>
                        </a>
                        <spring:url value="/settings/questions/${question.id}" var="delete_form_url" />
                        <form id="${question.type.code}_id_${question.id}" action="${delete_form_url}" method="post">
                            <input type="hidden" name="_method" value="DELETE" />
                        </form>
                    </c:if>
                </div>
            </div>

        </c:forEach>
    </section>

  </jsp:root>
