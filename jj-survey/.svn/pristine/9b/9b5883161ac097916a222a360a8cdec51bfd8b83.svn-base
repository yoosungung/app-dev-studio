/* 클라우드 장비표준분류코드 검색 레이어 팝업 */
.popup_layer_container table.tstyle_list { position:relative; width:100%; margin:6px 0;border:none; /* border-bottom:solid 1px #ededec; */ border-collapse:collapse; }
.popup_layer_container table.tstyle_list th {white-space:nowrap; text-align:center; vertical-align:middle; height:39px; overflow:hidden; color:#494949; border:1px solid #e5e5e5; border-top:3px solid #666; background-color:#f0f0f0; background-image:none;}
.popup_layer_container table.tstyle_list td {padding:0 0 0 15px; white-space:normal; text-align:center; border:1px solid #e5e5e5; color:#757575; vertical-align:top; }
.popup_layer_container table.tstyle_list td ul{height:300px ; overflow:scroll; overflow-x:hidden}
.popup_layer_container table.tstyle_list td ul li{padding:5px; border-bottom:1px solid #ededed; text-align:left; white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}
.popup_layer_container table.tstyle_list td ul li.on button{color:#488ad0;}
.popup_layer_container table.tstyle_list td ul li button:hover, .popup_layer_container table.tstyle_list td ul li button:focus{color:#488ad0;}
.popup_layer_container table.tstyle_list th.first {}
.popup_layer_container table.tstyle_list th.last {}
.popup_layer_container table.tstyle_list td.first{}
.popup_layer_container table.tstyle_list td.last{}
.popup_layer_container .btn_box_center{width:100%; text-align:center; margin:10px 0;}

