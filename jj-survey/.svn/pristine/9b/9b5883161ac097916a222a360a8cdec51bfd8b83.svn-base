@charset "utf-8";
/* CSS Document */
@import url(http://fonts.googleapis.com/earlyaccess/nanumgothic.css);



* { margin:0; padding:0; }
img { border:0px; }
html,body { height:100%; margin:0; padding:0; color:#444; font-size:12px; line-height:1.5em; font-family:'돋음', 'dotum', 'AppleGothic', 'Gulim', 'Tahoma', 'Verdana' , 'UnDotum', 'sans-serif'; }
a{text-decoration:none;color:#444; font-size:12px; line-height:1.5em;cursor:pointer}
ul, ol { list-style:none; }
ul li { list-style:none; }
hr, legend{display:none}
form, fieldset{padding:0; margin:0; border:none}
table{table-layout:fixed}
table, th, td {margin:0; padding:0; border:0; border-spacing:0; font-size:12px; border-collapse:separate; /* *border-collapse:collapse*/}
table caption{display:none}

input[type="text"]{margin:0; height:21px; vertical-align:middle}
input[type="password"]{margin:0; height:21px; vertical-align:middle}
input[type="radio"]{vertical-align:middle}
input[type="file"]{float:left; height:27px}
select{font-size:12px; color:#666}

button {border:0; padding:0; margin:0; cursor:pointer; }

.button.search *{width:50px; height:25px; text-indent: 100%; white-space: nowrap; overflow: hidden; background:url(../../../wcom/img/zeus2014/btn/btn_search.gif) no-repeat; }

.hidden{display:none !important; width:0; height:0; padding:0; margin:0;}
.ir{display:block; position:relative; top:0; font-size:11px; vertical-align:top; overflow:hidden; letter-spacing:-1px; z-index:-1}
.p_rtv{position:relative;}

.f_blue{color:#00b7ce !important}
.f_red{color:red !important}
.f_sky{color:#0182c0 !important}

.mb5{margin-bottom:5px !important}
.mb10{margin-bottom:10px !important}
.mb20{margin-bottom:20px !important}
.mb30{margin-bottom:30px !important}
.mt5{margin-top:5px !important}
.mt10{margin-top:10px !important}
.mt20{margin-top:20px !important}
.mt30{margin-top:30px !important}
.ml10{margin-left:10px !important}
.ml15{margin-left:15px !important}
.ml20{margin-left:20px !important}
.ml30{margin-left:30px !important}
.pr15{padding-right:15px !important}

.relative{position:relative;}



/* 공통 탭 */
.tab_type01{display:block; position:absolute; top:15px; right:20px;}
.tab_type01:after{clear:both; display:block; content:""}
