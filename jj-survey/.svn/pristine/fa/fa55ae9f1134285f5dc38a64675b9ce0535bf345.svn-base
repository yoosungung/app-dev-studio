.layer_notice{top:100px; left:300px; width:660px; background:#fff; border:2px solid #435363; border-radius:10px; box-shadow:3px 3px 3px rgba(0,0,0,0.3); z-index:100;}
.layer_notice .head_notice{padding:20px; background:#435363; border-radius:6px 6px 0px 0px;}
.layer_notice .head_notice .tit_notice{padding:1px 0px 1px 32px; width:540px; font:18px/24px "Nanum Gothic"; font-weight:normal; color:#fff; background:url(../../../wcom/img/zeus2014/cloud/icon_notice.png) left top no-repeat;}
.layer_notice .cont_notice{position:relative; max-height:600px; overflow:auto; margin:20px; border-top:none; border-radius:0px 0px 10px 10px;}
.layer_notice .cont_notice .file_notice{margin-bottom:15px; padding-bottom:15px; border-bottom:1px dashed #e2e2e2;}
.layer_notice .cont_notice .file_notice ul{}
.layer_notice .cont_notice .file_notice ul li{padding-bottom:2px;}
.layer_notice .cont_notice .file_notice ul li a{display:inline-block; padding-left:20px; font-size:11px; background:url(../../../wcom/img/zeus2014/cloud/icon_file.png) left top no-repeat;}
.layer_notice .cont_notice .file_notice ul li a:hover, .layer_notice .cont_notice .file_notice ul li a:focus, .layer_notice .cont_notice .file_notice ul li a:active{text-decoration:underline;}
.layer_notice .foot_notice{padding-right:20px; height:35px; line-height:35px; text-align:right; background:#ececec; border-top:1px solid #d8d8d8; border-radius:0px 0px 10px 10px;}
.layer_notice .foot_notice input[type="checkbox"]{vertical-align:top; margin:10px 5px 0px 0px;}
.layer_notice .btn_layerclose{display:block; position:absolute; top:30px; right:35px; width:21px; height:21px; background:url(../../../wcom/img/zeus2014/cloud/btn_layerclose.png) left top no-repeat; text-indent:-9999em; z-index:200;}