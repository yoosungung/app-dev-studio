@charset "utf-8";
/* CSS Document */

.skip{}
.skip a{position:absolute;top:-100px; left:0;display:block; margin:0;width:100%; padding:15px 0; text-align:center; background:#dbe7f6; z-index:9999;}
.skip a:hover, .skip a:active, .skip a:focus{top:0;}

.wrap{position:relative; width:100%; height:auto;}

/* header */
#header_wrap{position:relative; width:100%; height:auto; background:url(/wcom/img/zeus2014/cloud/bg_cloudnav.jpg) center bottom repeat-x;}
#header{position:relative; width:1024px; height:120px; margin:0 auto; zoom:1;}
#header:after{clear:both; display:block; content:""}

#header #gnb{display:block; position:relative; padding-top:5px; height:28px;}
#header #gnb .zeusnav{display:none; float:left;}
#header #gnb .zeusnav li{float:left;}
#header #gnb .zeusnav li a{display:block; margin-left:5px; padding-left:6px; line-height:33px; font-size:11px; color:#777; background:url(/wcom/img/zeus2014/main/bullet_line.gif) 0px 11px no-repeat;}
#header #gnb .zeusnav li a:hover, #header #gnb .zeusnav li a:focus, #header #gnb .zeusnav li a:active{color:#00b7ce;}
#header #gnb .zeusnav li a.first{margin:0; background:none;}

#header #gnb .tm{float:right;}
#header #gnb .tm li{position:relative; float:left;}
#header #gnb .tm li a{display:inline-block; height:20px; line-height:20px; color:#777; font-family:"Nanum Gothic","나눔고딕";}
#header #gnb .tm li .app{position:absolute; top:0px; left:0px; padding-left:25px; background:url(/wcom/img/zeus2014/main/sp_main.png) -450px 5px no-repeat;}
#header #gnb .tm li .login{margin:1px 0px 0px 12px; padding:0px 15px; height:18px; line-height:18px; background:#00b7ce; border-radius:20px; color:#fff; font-weight:bold;}
#header #gnb .tm li .join{margin-left:12px; padding-left:11px; background:url(/wcom/img/zeus2014/main/bullet_line.gif) 0px 6px no-repeat;}
#header #gnb .tm li .admin{margin-left:12px; padding-left:11px; background:url(/wcom/img/zeus2014/main/bullet_line.gif) 0px 6px no-repeat; font-weight: bold; color: #fa7828;}
#header #gnb .tm li span{display:inline-block; line-height:20px; font-family:"Nanum Gothic","나눔고딕";}
#header #gnb .tm li .allmenu{display:inline-block; vertical-align:top; margin:2px 9px 0px 13px; width:16px; height:16px; background:url(/wcom/img/zeus2014/public/sp_common.png) 0px -100px no-repeat; text-indent:-9999em;}
#header #gnb .tm li .allmenu:hover, #header #gnb .tm li .allmenu:focus, #header #gnb .tm li .allmenu:active, #header #gnb .tm li .allmenu:active{background-position:-50px -100px;}
#header #gnb .tm li .layer_menu{display:block; position:absolute; top:38px; right:0px; padding:10px 0px; width:198px; border:1px solid #d4d4d4; background:#f4f4f4; z-index:200;}
#header #gnb .tm li .layer_menu ul{background: url(/wcom/img/zeus2014/public/bg_layermenu.gif) left top repeat-y;float: left;width: 33.5%;}
#header #gnb .tm li .layer_menu ul.first{width: 33%;background: none;}

#header #gnb .tm li .layer_menu ul:after{clear:both; display:block; content:""}
#header #gnb .tm li .layer_menu ul li{padding:0px 24px; width:50px; height:25px;}
#header #gnb .tm li .layer_menu ul li a{line-height:25px;}
#header #gnb .tm li .layer_menu ul li a:hover, #header #gnb .tm li .layer_menu ul li a:focus, #header #gnb .tm li .layer_menu ul li a:active{text-decoration:underline;}
#header #gnb .tm li .layer_menu .layer_triangle{display:block; position:absolute; top:-6px; right:10px; width:12px; height:6px; background:url(/wcom/img/zeus2014/public/sp_common.png) -100px -100px no-repeat;}

#header #lnb{display:block;}
#header #lnb h1{display:block; position:absolute; top:20px; left:10px; line-height:30px;}
#header #lnb h1 a{font-family:'Nanum Gothic'; font-size:18px; }
#header #lnb h1 img{max-width:300px; max-height:30px; padding-right:5px; vertical-align:middle;}
#header #lnb h1 strong{padding-left:10px;}

#header #lnb #srch{position:absolute; right:3px; top:80px; height:24px; z-index:100;}
.srch_form{position:relative; height:28px;}
.srch_form input{float:left; padding:5px 10px; width:150px; height:18px; border:none; background:#a3d9e4;}
.srch_form .btn_srch{display:block; float:left; margin-left:5px; width:28px; height:28px; 
	background:url(/wcom/img/zeus2014/cloud/sp_cloud.png) 0px 0px no-repeat; border:0; text-indent:-9990em;
	}
#header #lnb #srch .zeuslink{display:block; position:absolute; top:-19px; right:0px; padding-right:10px; font-size:11px; background:url(/wcom/img/zeus2014/cloud/sp_cloud.png) 156px -46px no-repeat;}
#header #lnb #srch .zeuslink span{color:#00b7ce;}


#header #nav{display:block; position:relative; margin-top:35px; height:52px;}
#header #nav .nav_list{display:block; zoom:1;}
#header #nav .nav_list:after{clear:both; display:block; content:""}
#header #nav .nav_list li{display:block; position:relative; float:left; margin:7px 5px 0px 0px; }
#header #nav .nav_list li .nav_new{position:absolute; left:50%; top:-5px; margin:0 0 0 10px}
#header #nav .nav_list li a{display:block; position:relative; text-align:center; width:150px; height:38px; line-height:38px; font-family:"Nanum Gothic","나눔고딕"; color:#fff; font-size:16px;}
#header #nav .nav_list li a:hover, #header #nav .nav_list li a:focus, #header #nav .nav_list li a:active{color:#b6e9f0;}
#header #nav .nav_list li a.active{background:url(/wcom/img/zeus2014/cloud/black30.png); border-radius:20px;}



