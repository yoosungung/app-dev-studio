.choice select{padding-left:5px; width:180px; height:28px; line-height:24px; background:#fafafa; border:1px solid #e0e0e0;}

.tbl_calendar{position:relative; width:100%; border-top:2px solid #878f9f;}
.tbl_calendar thead th{height:38px; line-height:38px; background:#e8ebf2; border-bottom:1px solid #c9cfdc; border-left:1px solid #c9cfdc;}
.tbl_calendar tbody td{padding:8px; height:80px; vertical-align:top; color:#8f8f8f; border-bottom:1px solid #e0e0e0; border-left:1px solid #e0e0e0;}
.tbl_calendar strong{display:block; height:25px;}
.tbl_calendar .edge{border-left:none;}
.tbl_calendar .edge strong{color:#ff4200;}
.tbl_calendar .sun{color:#ff4200;}
.tbl_calendar .satur{color:#3f92d8;}

.icon_wrap{display:block; position:relative; padding-top:10px;}
.icon_wrap:after{display:block; clear:both; content:"";}
.icon_wrap ul{display:block; float:left; width:50%;}
.icon_wrap ul li{position:relative; margin-top:10px; padding-left:30px;}
.icon_wrap ul li .calicon{position:absolute; top:2px; left:0px;}
.calicon{display:inline-block; padding-left:24px; min-width:21px; height:21px; line-height:23px; font-size:11px; cursor:pointer;}
.calicon.icon1{background:url(/wcom/img/zeus2014/cloud/calicon1.png) no-repeat;}
.calicon.icon2{background:url(/wcom/img/zeus2014/cloud/calicon2.png) no-repeat;}
.calicon.icon3{background:url(/wcom/img/zeus2014/cloud/calicon3.png) no-repeat;}
.calicon.icon4{background:url(/wcom/img/zeus2014/cloud/calicon4.png) no-repeat;}
.calicon.icon5{background:url(/wcom/img/zeus2014/cloud/calicon5.png) no-repeat;}
.calicon.icon6{background:url(/wcom/img/zeus2014/cloud/calicon6.png) no-repeat;}

.bnr_wrap{position:relative; border-bottom:1px solid #e2e2e2;}

/*예약현황 상세 레이어 팝업 */
table.tbl_calendar tbody td .summery { position:absolute; top:50%; left:50%; margin:-140px 0 0 -160px;  width:320px; height:280px; border:1px solid #dddddd; background:#f5f6f6; z-index:2002; }
table.tbl_calendar tbody td .summery .title { position:relative; padding-left:15px; height:38px; border-bottom:1px solid #dddddd; color:#505050; font-weight:bold; line-height:38px; }
table.tbl_calendar tbody td .summery .title .date { color:#4791de; padding-left:15px; font-weight:normal; display:inline;  }
table.tbl_calendar tbody td .summery .title button.close { position:absolute; right:10px; top:8px; padding:0; margin:0; background:none; border:1px solid #dcdcdc; }
table.tbl_calendar tbody td .summery .list { position:relative; padding:15px; height:211px; color:#505050; overflow-x:hidden; overflow-y:scroll; }
table.tbl_calendar tbody td .summery .list ul { position:relative; color:#505050; }
table.tbl_calendar tbody td .summery .list ul li { position:relative; color:#646464; /* background-position:1px 9px; */ background-repeat:no-repeat; padding:0px 0 5px 29px; font-weight:normal; }
table.tbl_calendar tbody td .summery .list ul li .equip_nm { color:#505050; }
table.tbl_calendar tbody td .summery .list ul li .model_nm { color:#505050; }
table.tbl_calendar tbody td .summery .list ul li .user_nm{ color:#505050; font-weight:bold; }

table.tbl_calendar tbody td .event.type_01 { background-image:url(/wcom/img/cloud/icon/resv_exp_01.png); }
table.tbl_calendar tbody td .event.type_01 strong { color:#f37f02; }
table.tbl_calendar tbody td .summery li.type_01 {background:url(/wcom/img/zeus2014/cloud/calicon4.png);}

table.tbl_calendar tbody td .event.type_02 { background-image:url(/wcom/img/cloud/icon/resv_exp_02.png); }
table.tbl_calendar tbody td .event.type_02 strong { color:#399ce9; }
table.tbl_calendar tbody td .summery li.type_02 {background:url(/wcom/img/zeus2014/cloud/calicon5.png);}

table.tbl_calendar tbody td .event.type_03 { background-image:url(/wcom/img/cloud/icon/resv_exp_03.png); }
table.tbl_calendar tbody td .event.type_03 strong { color:#11b700; }
table.tbl_calendar tbody td .summery li.type_03 {background:url(/wcom/img/zeus2014/cloud/calicon6.png);}

table.tbl_calendar tbody td .event.type_04 { background-image:url(/wcom/img/cloud/icon/resv_exp_04.png); }
table.tbl_calendar tbody td .event.type_04 strong { color:#11b700; }
table.tbl_calendar tbody td .summery li.type_04 {background:url(/wcom/img/zeus2014/cloud/calicon1.png);}

