/* 장비 예약 하기 : 장비 상세 보기, 빠른 예약에서 사용 */
.request_area { position:relative; padding:15px;  }
.request_area h2 { position:relative; font-size:16px; color:#1c85ce; padding-top:4px; }
.request_area fieldset { border:0px; margin:0px; padding:0px; padding-top:15px; }
.request_area fieldset legend { position:absolute; left:-1px; top:-1px; width:1px; height:1px; overflow:hidden; border:0px; margin:0px; padding:0px; }
.request_area fieldset.request_user_infor {}
.request_area fieldset.request_user_infor table.ajax_form { position:relative; width:100%; border:none; border-collapse:collapse; border-top:1px solid #e5e5e5; }
.request_area fieldset.request_user_infor table.ajax_form th { background:#f2f2f2; border-top:1px solid #d9d9d9; line-height:2em; vertical-align:top; padding-left:15px; width:75px; color:#838282; font-size:12px; font-weight:bold; text-align:left; }
.request_area fieldset.request_user_infor table.ajax_form td { background:#f2f2f2; border-top:1px solid #d9d9d9; line-height:2em; vertical-align:bottom; color:#656565; font-size:12px; font-weight:bold; }
.request_area fieldset.request_user_infor .input_guide { position:relative; text-align:right; color:#868686; font-size:11px; padding-bottom:8px; }
.request_area fieldset.request_user_infor .required { font-size:12px; color:#f95e00;}

.request_area fieldset.request_infor { border-bottom:1px solid #d9d9d9; }
.request_area fieldset.request_infor table.ajax_form { position:relative; width:100%; border:none; border-collapse:collapse; }
.request_area fieldset.request_infor table.ajax_form th { padding-left:15px; width:75px; color:#838282; font-size:12px; font-weight:bold; vertical-align:top; padding-top:10px; text-align:left; }
.request_area fieldset.request_infor table.ajax_form td { color:#656565; font-size:12px; padding:4px 0; }
.request_area fieldset.request_infor table.ajax_form td input { width:80px; height:28px; line-height:28px; font-size:14px; border:1px solid #e1e1e1; }
.request_area fieldset.request_infor table.ajax_form td input.input_full { width:99%; }
.request_area fieldset.request_infor table.ajax_form td textarea { ine-height:12px; font-size:14px; border:1px solid #e1e1e1; }
.request_area fieldset.request_infor table.ajax_form td textarea.input_full { width:99%; }
.request_area fieldset.request_infor table.ajax_form td .guide { position:relative; text-align:left; color:#898989; font-size:11px; display:block; padding-top:5px; }
.request_area fieldset.request_infor table.ajax_form td .inline_guide { text-align:left; color:#898989; font-size:11px; }
.request_area fieldset.request_infor table.ajax_form td .tip { position:relative; display:block; text-align:left; color:#898989; font-size:11px; background:url(/wcom/img/equip/tip_icon.png) left 6px no-repeat; padding-top:5px; padding-left:38px; }
.request_area fieldset.request_infor table.ajax_form2 { position:relative; width:100%; border:none; border-collapse:collapse; }
.request_area fieldset.request_infor table.ajax_form2 th { padding-left:15px; width:75px; color:#838282; font-size:12px; font-weight:bold; vertical-align:top; padding-top:10px; text-align:left; }
.request_area fieldset.request_infor table.ajax_form2 td { color:#656565; font-size:12px; padding:4px 0; }
.request_area fieldset.request_infor table.ajax_form2 td input { width:80px; height:20px; line-height:20px; font-size:13px; border:1px solid #e1e1e1; }
.request_area fieldset.request_infor table.ajax_form2 td select { width:60px; height:21px; line-height:20px; font-size:13px; border:1px solid #e1e1e1; }
.request_area fieldset.request_infor table.ajax_form2 td input.input_full { width:99%; }
.request_area fieldset.request_infor table.ajax_form2 td textarea { ine-height:12px; font-size:14px; border:1px solid #e1e1e1; }
.request_area fieldset.request_infor table.ajax_form2 td textarea.input_full { width:99%; }
.request_area fieldset.request_infor table.ajax_form2 td .guide { position:relative; text-align:left; color:#898989; font-size:11px; display:block; padding-top:5px; }
.request_area fieldset.request_infor table.ajax_form2 td .inline_guide { text-align:left; color:#898989; font-size:11px; }
.request_area fieldset.request_infor table.ajax_form2 td .tip { position:relative; display:block; text-align:left; color:#898989; font-size:11px; background:url(/wcom/img/equip/tip_icon.png) left 6px no-repeat; padding-top:5px; padding-left:38px; }
.request_area fieldset.request_infor .required { font-size:12px; color:#f95e00; margin-left:-7px; }
.request_area .request_guide { position:relative; padding-top:4px; background:url(/wcom/img/equip/guide_icon.gif) left 10px no-repeat; padding-left:15px; }
.request_area .buttons { text-align:center; }
