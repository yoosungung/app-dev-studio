.over_box{position:relative; display:block; width:auto; height:auto; padding:0 20px; border:1px solid #ccc;}
.over_box2{position:relative; display:block; width:auto; height:auto;}
.x_scroll{overflow-x:auto}
.y_scroll{overflow-y:auto}

.tab {position:relative; margin:0; padding:0; border-bottom:1px solid #ccc; clear:both;}
.tab:after{clear:both; display:block; content:""}
.tab ul {float:left; margin:1px 0 0 0; padding:0; height:32px;}
.tab ul:after{clear:both; display:block; content:""}
.tab ul li {float:left;	text-align:center; height:32px; background:#ececec;	border-top:1px solid #ccc;border-left:1px solid #ccc;border-right:1px solid #ccc;display:inline;margin-right:-1px;}
.tab ul a,
.tab ul span,
.tab ul strong{font-size:13px; color:#000;line-height:32px; height:32px; display:block; padding:0 11px;}
.tab ul a:hover{color:#000; background:#ccc; text-decoration:none;}
.tab ul li.tab_on a,
.tab ul li.tab_on span,
.tab ul li.tab_on strong{color:#000; background:#fff;line-height:32px; height:32px; padding:0 10px; font-weight:bold}

.tab ul li a.f_zeus{ font-family:'돋음' !important;}
.tab ul li.tab_on a.f_zeus,
.tab ul a.f_zeus:hover{color:#00b6cd !important; font-family:'돋음' !important;}

.tab ul li.tab_on a.f_ntis,
.tab ul a.f_ntis:active{color:#092da0 !important}

.tab ul li.tab_on a.f_redt,
.tab ul a.f_redt:active{color:#ff0000 !important}


/* 20150320. ntis.table-style CSS */
.bln{border-left:0 !important}
.brn{border-right:0 !important}
.non_sell{border-top:0 !important}
.bg_g1{background:#f4f4f4 !important}
.bg_g2{background:#fafafa !important}
.bg_point{background:#f4f8f9 !important;}

.tbl03 {width:100%; border-top:2px solid #cecece}
.tbl03 th{padding:0 5px; height:30px; border-bottom:1px solid #cecece; color:#595959; background:#ededed url(../../wcom/images/contents/bg_tbl_header.gif) repeat-x 0 0; font-size:14px;}
.tbl03 td{padding:10px 5px; border:1px solid #cecece;font-size:12px;}

/* 20150320. ntis.button CSS */
.btn_v23{font-weight:normal; font-size:12px; background:#fff url(../../wcom/images/common/btn_v23.gif) no-repeat left top; color:#5a5a5a;line-height:22px !important; height:23px !important;  display:inline-block;	margin:0 1px; padding:0 0 0 10px !important;text-align:left; vertical-align:middle; text-decoration:none; border:0 !important; }
.btn_v23 span { display:inline-block; background:#fff url(../../wcom/images/common/btn_v23.gif) no-repeat right top; line-height:22px !important; height:23px !important;  padding:0px 10px 0 0 !important; vertical-align: top; }
.btn_v23:hover {background-position:left bottom; }
.btn_v23:hover span {background-position:right bottom; color:#000; cursor:pointer;}

.btn_v23.active{background:#fff url(../../wcom/images/common/btn_v23_active.gif) no-repeat left top; color:#fff;}
.btn_v23.active span {display:inline-block; background:#fff url(../../wcom/images/common/btn_v23_active.gif) no-repeat right top;}
.btn_v23.active:hover {background-position:left bottom; }
.btn_v23.active:hover span {background-position:right bottom; color:#fff; cursor:pointer;}
