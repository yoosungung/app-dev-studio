/* 목록 */
table.tstyle_list { position:relative; width:100%; border:none; /* border-bottom:solid 1px #ededec; */ border-collapse:collapse;table-layout:fixed}
table.tstyle_list th {white-space:nowrap; text-align:center; vertical-align:middle; height:39px; overflow:hidden; color:#3f72b2; line-height:14px; }
table.tstyle_list th a{white-space:nowrap; text-align:center; vertical-align:middle; height:39px; overflow:hidden; color:#3f72b2; line-height:14px; }
table.tstyle_list th a.up{ background:url(/wcom/img/icon/icon_arr_up.png) no-repeat right 10px; padding-right:10px;}
table.tstyle_list th a.down{ background:url(/wcom/img/icon/icon_arr_down.png) no-repeat right 10px; padding-right:10px;}
table.tstyle_list td {white-space:normal; text-align:center; padding:10px 5px; border-bottom:solid 1px #ededec; color:#757575; }
table.tstyle_list td a{color:#757575;}
table.tstyle_list td a.haveFile { padding-left:15px; background:url(/wcom/img/icon/icon_file.gif) left center no-repeat; }
table.tstyle_list td a.link{color:#488ad0;}
table.tstyle_list td span,table.tstyle_list td label,table.tstyle_list td button {color:#757575;}
table.tstyle_list td p.blue{color:#4791de;}
table.tstyle_list thead th {background:url(/wcom/img/bg/list_header_bg.gif) center 4px no-repeat;}
table.tstyle_list thead th.first { background:url(/wcom/img/bg/list_header_bg.gif) left 4px no-repeat; padding-left:5px;}
table.tstyle_list thead th.last { background:url(/wcom/img/bg/list_header_bg.gif) right 4px no-repeat;padding-right:5px;}
table.tstyle_list td.first{}
table.tstyle_list td.last{}
table.tstyle_list tbody tr.deleted { text-decoration:line-through; color:red; }
table.tstyle_list tbody th { text-align:right; padding:3px 0;}
table.tstyle_list tbody th.title, table.tstyle_list tbody td.title { white-space:normal; text-align:left; }
table.tstyle_list tbody td.cafe_notice{background:url(/wcom/img/icon/icon_cafe_notice.gif) no-repeat 5px 13px; padding-left:25px;}
table.tstyle_list tbody td.title a.reply { background:url(/wcom/img/icon/icon_reply.gif) left center no-repeat; padding-left:12px; }
table.tstyle_list tbody td.word_reduce {width:100%; white-space:nowrap; overflow:hidden; text-overflow:ellipsis}
table.tstyle_list tbody td.deleted { text-decoration:line-through; color:#9d9d9d; }
table.tstyle_list tbody td.deleted a { color:#9d9d9d; }
table.tstyle_list tbody td.accuse { text-decoration:line-through; color:#9d9d9d; }
table.tstyle_list tbody td.accuse a { color:#9d9d9d; }
/* 소모임가입사유 */
table.tstyle_list td div.cause{margin:0; padding:0; float:left;width:96%;text-align:left; padding:10px 2%; border:1px solid #f4f4f4; background-color:#fafafa;}
table.tstyle_list td div.cause p{padding:0; margin:0;}
table.tstyle_list td div.cause p.title{float:left; font-weight:bold;}
table.tstyle_list td div.cause p.txt{float:left; margin-left:10px;}


