.btn_more{display:block; position:absolute; top:8px; right:7px; width:30px; height:30px; background:url(../../wcom/img/zeus2014/main/sp_main.png) -392px 8px no-repeat;}
.btn_more.type2{top:11px; right:-5px;}
.btn_more.type3{top:5px; right:-5px;}
.btn_more.type4{top:0px; right:-5px;}
.btn_more.type5{top:20px; right:12px; width:30px; height:30px; background:url(../../wcom/img/zeus2014/main/sp_main.png) -392px 8px no-repeat;}


.scorestar{display:inline-block; vertical-align:top; margin-top:8px; width:58px; height:10px; background:url(../../wcom/img/zeus2014/main/sp_star.png) no-repeat; text-indent:-9999em;}
.scorestar.star00{background-position:0px 0px;}
.scorestar.star05{background-position:0px -50px;}
.scorestar.star10{background-position:0px -100px;}
.scorestar.star15{background-position:0px -150px;}
.scorestar.star20{background-position:0px -200px;}
.scorestar.star25{background-position:0px -250px;}
.scorestar.star30{background-position:0px -300px;}
.scorestar.star35{background-position:0px -350px;}
.scorestar.star40{background-position:0px -400px;}
.scorestar.star45{background-position:0px -450px;}
.scorestar.star50{background-position:0px -500px;}


.pagenav_type1{display:block; height:27px; text-align:center; font-size:0;}
.pagenav_type1 a{display:inline-block; margin:0px 2px; width:25px; height:25px; vertical-align:top; line-height:25px; text-align:center; font-size:12px; border:1px solid #ebecee;}
.pagenav_type1 .active{background:#a7a7a7; border:1px solid #979797; color:#fff; font-weight:bold;}
.pagenav_type1 .arrow{background:url(../../wcom/img/zeus2014/cloud/sp_cloud.png) no-repeat;}
.pagenav_type1 .arrow.fastprev{margin-right:8px; background-position:0px -150px;}
.pagenav_type1 .arrow.fastnext{margin-left:8px; background-position:-50px -150px;}
.pagenav_type1 .arrow.prev{margin-right:11px; background-position:-100px -150px;}
.pagenav_type1 .arrow.next{margin-left:11px; background-position:-150px -150px;}



/* 구 제우스  css*/
.del{background:url(../../wcom/img/zeus2014/common/bg_gray_dot.gif) repeat-x 0 50% !important}
.f_blue{color:#00b7ce !important}
.f_red{color:red !important}
.f_sky{color:#0182c0 !important}
.ir{display:block; position:relative; top:0; font-size:11px; vertical-align:top; overflow:hidden; letter-spacing:-1px; z-index:-1}

.fl{float:left !important}
.fr{float:right !important}

.google_marker_label { position:absolute; height:53px; text-align:left; padding-right:7px; cursor:default;}
.google_marker_label .label_text { position:relative; background:url(../../wcom/img/zeus2014/content/partner/info_window.png) left top no-repeat transparent; white-space:nowrap; overflow:hidden; text-overflow:ellipsis; }
.google_marker_label .label_text { color:#fff; }
.google_marker_label .label_text a { float:left; height:32px; padding:11px 6px 10px 4px; display:inline; color:#fff; }
.google_marker_label .label_text a.icon { background:url(../../wcom/img/zeus2014/content/partner/btn_arrow_blue.png) 4px 14px no-repeat transparent; padding-left:20px; }
.google_marker_label .close_btn { position:absolute; text-align:left; right:0px; padding-top:7px; height:46px; width:7px; background:url(../../wcom/img/zeus2014/content/partner/info_window.png) right top no-repeat transparent; }

.btn_weather_box{position:relative; display:block; float:right; margin:-30px 0 0 0; width:200px; height:26px; line-height:26px; text-align:right;}
.btn_weather_box:after{clear:both; display:block; content:""}
.btn_weather_box.lnb{/* position:relative; */} 
#lnb .lnb_date{position:absolute;  right:107px; top:0; color:#000; display:block; font-family:Tahoma; text-align:right;height:26px; line-height:26px; }
/*@media screen and (-webkit-min-device-pixel-ratio:0){.lnb .lnb_date{left:15px}}*/
.btn_weather_box.lnb .lnb_date .day{display:block; float:left; font-size:11px;height:26px; line-height:26px; }
.btn_weather_box.lnb .lnb_date strong{display:block; float:left;height:26px; line-height:26px; font-weight:normal}

.btn_weather_box.lnb .lnb_weather{position:relative; height:26px; line-height:26px;  overflow:hidden; width:115px; float:right; font-family:Tahoma;}
.btn_weather_box.lnb .lnb_weather ul{position:absolute; right:0;padding:0;font-size:11px;text-align:right;vertical-align:middle;}
.btn_weather_box.lnb .lnb_weather ul li{position:relative;height:26px; line-height:26px; overflow:hidden;}
.btn_weather_box.lnb .lnb_weather ul li:after{clear:both; display:block; content:""}
.btn_weather_box.lnb .lnb_weather ul li img{margin-top:-2px;vertical-align:middle;}
.btn_weather_box.lnb .lnb_weather ul li .area{float:left; display:block; width:70px;  padding:0; text-align:right; vertical-align:top}
.btn_weather_box.lnb .lnb_weather ul li .w_info{float:left;display:block;  width:36px;padding:0;margin:0;text-align:right;vertical-align:top}
.btn_weather_box.lnb .lnb_weather ul li .temperature{float:left; display:block; width:33px; padding:0;margin:0;color:#000;vertical-align:top}

.btn_weather_box.lnb .lnb_clock{position:absolute;right:0;top:-10px;font-size:14px;color:#fff;}
.btn_weather_box.lnb .lnb_clock .number_box{position:relative;display:inline-block;float:left;margin:10px 1px 0;width:16px;height:23px;overflow:hidden;}
.btn_weather_box.lnb .lnb_clock .number_box.colon{width:8px;}
.btn_weather_box.lnb .lnb_clock strong{position:relative;width:18px;height:26px;line-height:26px;overflow:hidden;background:url(../../wcom/img/zeus2014/bg_num.png) no-repeat;}
.btn_weather_box.lnb .lnb_clock strong span{display:inline-block;text-indent:-999px;overflow:hidden;}
.btn_weather_box.lnb .lnb_clock strong.num_colon{position:absolute;width:8px; left:0px; background-position:-3px -390px;}
.btn_weather_box.lnb .lnb_clock strong.num_x{position:absolute;width:18px;height:396px;left:0px;line-height:36px;overflow:hidden;background:url(../../wcom/img/zeus2014/bg_num.png) no-repeat;background-position:0 0;}
.btn_weather_box.lnb .lnb_clock strong.num_x span{display:inline-block;text-indent:-999px;overflow:hidden}

/* .ui-dialog.ui-widget.ui-widget-content.ui-corner-all{left:50% !important; margin-left:-350px !important;} */

.layer_notice{top:100px; left:300px; width:660px; background:#fff; border:2px solid #435363; border-radius:10px; box-shadow:3px 3px 3px rgba(0,0,0,0.3); z-index:100;}
.layer_notice .head_notice{padding:20px; background:#2992a1; border-radius:6px 6px 0px 0px;}
.layer_notice .head_notice .tit_notice{padding:1px 0px 1px 32px; width:540px; font:18px/24px "Nanum Gothic"; font-weight:normal; color:#fff; background:url(../../wcom/img/zeus2014/main/icon_notice2.png) left top no-repeat;}
.layer_notice .cont_notice{position:relative; max-height:600px; overflow:auto; margin:20px; border-top:none; border-radius:0px 0px 10px 10px;}
.layer_notice .cont_notice .file_notice{margin-bottom:15px; padding-bottom:15px; border-bottom:1px dashed #e2e2e2;}
.layer_notice .cont_notice .file_notice ul{}
.layer_notice .cont_notice .file_notice ul li{padding-bottom:2px;}
.layer_notice .cont_notice .file_notice ul li a{display:inline-block; padding-left:20px; font-size:11px; background:url(../../wcom/img/zeus2014/cloud/icon_file.png) left top no-repeat;}
.layer_notice .cont_notice .file_notice ul li a:hover, .layer_notice .cont_notice .file_notice ul li a:focus, .layer_notice .cont_notice .file_notice ul li a:active{text-decoration:underline;}
.layer_notice .foot_notice{padding-right:20px; height:35px; line-height:35px; text-align:right; background:#ececec; border-top:1px solid #d8d8d8; border-radius:0px 0px 10px 10px;}
.layer_notice .foot_notice input[type="checkbox"]{vertical-align:top; margin:10px 5px 0px 0px;}
.layer_notice .btn_layerclose{display:block; position:absolute; top:30px; right:35px; width:21px; height:21px; background:url(../../wcom/img/zeus2014/cloud/btn_layerclose.png) left top no-repeat; text-indent:-9999em; z-index:200;}

