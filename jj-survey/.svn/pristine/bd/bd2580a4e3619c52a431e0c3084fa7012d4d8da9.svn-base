         url('../../wcom/font/NanumBarunGothicBold.eot?#iefix') format('embedded-opentype'),
         url('../../wcom/font/NanumBarunGothicBold.woff') format('woff'),
         url('../../wcom/font/NanumBarunGothicBold.ttf') format('truetype');
    font-weight: normal;
    font-style: normal; 
}
