* { margin:0; padding:0; }
img { border:0px; }
html,body { height:100%; margin:0; padding:0; color:#191919; font-size:12px; line-height:1.5em; font-family:'돋음', AppleGothic, dotum, Gulim, Tahoma, Verdana , UnDotum, sans-serif; }
a{text-decoration:none;color:#444; font-size:12px; line-height:1.5em;cursor:pointer}
ul, ol { list-style:none; }
ul li { list-style:none; }
hr, legend{display:none}
form, fieldset{padding:0; margin:0; border:none}
table{table-layout:fixed}
table, th, td {margin:0; padding:0; border:0; border-spacing:0; font-size:12px; border-collapse:separate; /* *border-collapse:collapse*/}
table caption{display:none}

input[type="text"]{margin:0; height:21px; vertical-align:middle}
input[type="password"]{margin:0; height:21px; vertical-align:middle}
input[type="radio"]{vertical-align:middle}
input[type="file"]{float:left; height:27px}
select{font-size:12px; color:#666}
button {border:0; padding:0; margin:0; cursor:pointer; }

.hidden{display:none !important; width:0; height:0; padding:0; margin:0;}

.mb15{margin-bottom:15px !important}
.mb30{margin-bottom:30px !important}
.mt10{margin-top:10px !important}
.mt5{margin-top:5px !important}
.mt30{margin-top:30px !important}
.mr5{margin-right:5px !important}
.pt10{padding-top:10px;}
.pt20{padding-top:20px;}
.pt30{padding-top:30px;}


/* NanumBarunGothic */
@font-face {
    font-family:'NanumBarunGothic';
    src: url('../../wcom/font/NanumBarunGothic.eot');
    src: local('?'),
         url('../../wcom/font/NanumBarunGothic.eot?#iefix') format('embedded-opentype'),
         url('../../wcom/font/NanumBarunGothic.woff') format('woff'),
